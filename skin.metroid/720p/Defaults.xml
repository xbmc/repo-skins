  <default type="togglebutton">
    <left>0</left>
    <top>0</top>
    <width>150</width>
    <height>48</height>

    <font>DefaultButton</font>
    <textcolor>settings</textcolor>
    <focusedcolor>White</focusedcolor>
    <disabledcolor>FF464646</disabledcolor>
    <texturenofocus>-</texturenofocus>
