<?xml version="1.0" encoding="UTF-8"?>
<includes>
  
  <default type="label">
    <left>0</left>
    <top>0</top>
 
    <height>30</height>
    <align>left</align>
    <aligny>center</aligny>
    <font>list</font>
    <!-- textcolor>ACFFFFFF</textcolor -->
    <textcolor>settings</textcolor>
    <focusedcolor>FFE1E1E1</focusedcolor>
    <selectedcolor>FFc9f1ff</selectedcolor>
    <disabledcolor>FF464646</disabledcolor>
