<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1116">
	<visible>!String.IsEmpty(Window(home).Property(ShowBusy))</visible>
	<zorder>999</zorder>
	<include>PropertyIncludes</include>
	<animation effect="fade" start="100" end="0" time="200" delay="100">WindowClose</animation>
	<controls>
		<control type="group">
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture colordiffuse="background_diffuse">common/white.jpg</texture>
				<aspectratio>stretch</aspectratio>
			</control>
			<include>BusySpinner</include>
		</control>
	</controls>
</window>