<includes>
	<!-- Move function -->
	<include name="PositionSelectorMoveFunc">
		<!-- temporary save items -->
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move) + !String.IsEmpty(Container(211).ListItem.Property(item))">Skin.SetString(ItemLabel,$INFO[Container(211).ListItem.Property(itemlabel)])</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move) + !String.IsEmpty(Container(211).ListItem.Property(item))">Skin.SetString(ItemType,$INFO[Container(211).ListItem.Property(item)])</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move) + !String.IsEmpty(Container(211).ListItem($PARAM[direction]).Property(item))">Skin.SetString(TempItemLabel,$INFO[Container(211).ListItem($PARAM[direction]).Property(itemlabel)])</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move) + !String.IsEmpty(Container(211).ListItem($PARAM[direction]).Property(item))">Skin.SetString(TempItemType,$INFO[Container(211).ListItem($PARAM[direction]).Property(item)])</onclick>
		<!-- save current items to next position -->
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Skin.SetString($VAR[PositionselectorGroup]_widget_pos_$INFO[Container(211).ListItem($PARAM[direction]).Property(pos)].label,$INFO[Skin.String(ItemLabel)])</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Skin.SetString($VAR[PositionselectorGroup]_widget_pos_$INFO[Container(211).ListItem($PARAM[direction]).Property(pos)],$INFO[Skin.String(ItemType)])</onclick>
		<!-- save temporary items to current postion -->
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Skin.SetString($VAR[PositionselectorGroup]_widget_pos_$INFO[Container(211).ListItem.Property(pos)].label,$INFO[Skin.String(TempItemLabel)])</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Skin.SetString($VAR[PositionselectorGroup]_widget_pos_$INFO[Container(211).ListItem.Property(pos)],$INFO[Skin.String(TempItemType)])</onclick>
		<!-- clear temp strings -->
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Skin.Reset(TempItemLabel)</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Skin.Reset(TempItemType)</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Skin.Reset(ItemLabel)</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Skin.Reset(ItemType)</onclick>
		<!-- move and secure a new unwanted input -->
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">Control.Move(211,$PARAM[direction])</onclick>
		<onclick condition="!String.IsEmpty(Container(211).ListItemNoWrap($PARAM[direction]).Label) + !System.HasAlarm(move)">AlarmClock(move,noop,00:01,silent)</onclick>
	</include>

