<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>11</defaultcontrol>
	<zorder>999</zorder>
	<include>PropertyIncludes</include>
	<controls>
		<!-- Hidden Control-->
		<control type="progress" id="32">
			<include>HiddenObject</include>
		</control>
		<!-- Dialog -->
		<control type="group">
			<animation type="WindowOpen" reversible="false">
				<effect type="slide" start="0,-200" end="0,0" delay="100" tween="circle" easin="out" time="400" />
				<effect type="fade" delay="160" end="100" time="400" />
			</animation>
			<animation type="WindowClose" reversible="false">
				<effect type="slide" start="0,0" end="0,-200" easing="in" tween="circle" easin="out" time="400" />
				<effect type="fade" start="100" end="0" time="400" />
			</animation>
			<width>550</width>
			<height>240</height>
			<right>30</right>
			<top>30</top>
			<!-- Background image -->
			<control type="image">
				<texture colordiffuse="context_color" border="65">dialogs/notification/bg.png</texture>
			</control>
			<!-- Slider -->
			<control type="group">
				<centertop>50%</centertop>
				<left>80</left>
				<right>80</right>
				<height>26</height>
				<control type="image">
					<top>8</top>
					<description>Slider</description>
					<width>100%</width>
					<height>10</height>
					<texture colordiffuse="scrollbar_background">common/white.jpg</texture>
				</control>
				<control type="slider" id="11">
					<top>5</top>
					<width>100%</width>
					<height>8</height>
					<onup>11</onup>
					<ondown>11</ondown>
					<onleft>11</onleft>
					<onright>11</onright>
				</control>
			</control>
			<!-- Heading -->
			<control id="10" type="label">
				<top>65</top>
				<left>80</left>
				<right>80</right>
				<font>Med24</font>
				<align>center</align>
				<aligny>top</aligny>
				<textcolor>context_textcolor</textcolor>
			</control>
			<!-- Details -->
			<control id="12" type="label">
				<bottom>65</bottom>
				<height>25</height>
				<left>80</left>
				<right>80</right>
				<font>Reg22</font>
				<align>center</align>
				<aligny>top</aligny>
				<scroll>true</scroll>
				<textcolor>context_textcolor</textcolor>
			</control>
		</control>
	</controls>
</window>
