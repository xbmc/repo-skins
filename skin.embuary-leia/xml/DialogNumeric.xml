<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- virtualkeyboard -->
	<defaultcontrol always="true">21</defaultcontrol>
	<zorder>999</zorder>
	<include>PropertyIncludes</include>
	<controls>
		<!-- Background -->
		<include content="DiffuseMask">
			<param name="color">background_diffuse</param>
			<param name="visible">!Window.IsVisible(virtualkeyboard) + !Window.IsVisible(1113)</param>
		</include>
		<!-- Control group -->
		<control type="grouplist">
			<include>Dialog_PopupOpenClose</include>
			<centerleft>50%</centerleft>
			<width>788</width>
			<height>1080</height>
			<orentation>vertical</orentation>
			<itemgap>0</itemgap>
			<align>center</align>
			<!-- Heading -->
			<control type="group">
				<height>52</height>
				<control id="1" type="label">
					<left>5</left>
					<right>5</right>
					<height>50</height>
					<font>med30</font>
				</control>
			</control>
			<!-- Edit -->
			<control type="group">
				<height>40</height>
				<control id="4" type="label">
					<left>5</left>
					<right>5</right>
					<height>40</height>
					<scroll>true</scroll>
					<font>Reg24</font>
					<disabledcolor>lightgrey</disabledcolor>
					<focusedcolor>$VAR[AccentColor]</focusedcolor>
				</control>
			</control>
			<!-- Edit Text line -->
			<control type="group">
				<height>2</height>
				<control type="image">
					<left>5</left>
					<right>5</right>
					<height>2</height>
					<texture colordiffuse="lightgrey">common/white.jpg</texture>
				</control>
				<control type="image">
					<left>5</left>
					<right>5</right>
					<height>2</height>
					<texture colordiffuse="$VAR[AccentColor]">common/white.jpg</texture>
					<include>WindowOpen_GreenBar</include>
				</control>
			</control>
			<control type="label">
				<height>16</height>
			</control>
			<!-- Keys -->
			<!-- First row -->
			<control id="1000" type="grouplist">
				<width>788</width>
				<height>161</height>
				<itemgap>12</itemgap>
				<onleft>11</onleft>
				<onright>16</onright>
				<orientation>horizontal</orientation>
				<!-- '1' -->
				<control id="11" type="button">
					<ondown>16</ondown>
					<label>12311</label>
					<include>NumericButtons</include>
				</control>
				<!-- '2' -->
				<control id="12" type="button">
					<ondown>17</ondown>
					<label>12312</label>
					<include>NumericButtons</include>
				</control>
				<!-- '3' -->
				<control id="13" type="button">
					<ondown>18</ondown>
					<label>12313</label>
					<include>NumericButtons</include>
				</control>
				<!-- '4' -->
				<control id="14" type="button">
					<ondown>19</ondown>
					<label>12314</label>
					<include>NumericButtons</include>
				</control>
				<!-- '5' -->
				<control id="15" type="button">
					<ondown>10</ondown>
					<label>12315</label>
					<include>NumericButtons</include>
				</control>
			</control>
			<!-- Second row -->
			<control id="2000" type="grouplist">
				<width>788</width>
				<height>161</height>
				<itemgap>12</itemgap>
				<onup>1000</onup>
				<onleft>15</onleft>
				<onright>20</onright>
				<orientation>horizontal</orientation>
				<!-- '6' -->
				<control id="16" type="button">
					<onup>11</onup>
					<ondown>20</ondown>
					<label>12316</label>
					<include>NumericButtons</include>
				</control>
				<!-- '7' -->
				<control id="17" type="button">
					<onup>12</onup>
					<ondown>22</ondown>
					<label>12317</label>
					<include>NumericButtons</include>
				</control>
				<!-- '8' -->
				<control id="18" type="button">
					<onup>13</onup>
					<ondown>23</ondown>
					<label>12318</label>
					<include>NumericButtons</include>
				</control>
				<!-- '9' -->
				<control id="19" type="button">
					<onup>14</onup>
					<ondown>301</ondown>
					<label>12319</label>
					<include>NumericButtons</include>
				</control>
				<!-- '0' -->
				<control id="10" type="button">
					<onup>15</onup>
					<ondown>21</ondown>
					<label>12310</label>
					<include>NumericButtons</include>
				</control>
			</control>
			<!-- Third row -->
			<control id="3000" type="grouplist">
				<width>788</width>
				<height>68</height>
				<itemgap>12</itemgap>
				<onleft>10</onleft>
				<onright>21</onright>
				<orientation>horizontal</orientation>
				<!-- Previous -->
				<control id="20" type="button">
					<onup>16</onup>
					<width>148</width>
					<font>Material24</font>
					<textcolor>text_white</textcolor>
					<label></label>
					<include>KeyboardButtons</include>
				</control>
				<!-- Next -->
				<control id="22" type="button">
					<onup>17</onup>
					<width>148</width>
					<font>Material24</font>
					<textcolor>text_white</textcolor>
					<label></label>
					<include>KeyboardButtons</include>
				</control>
				<!-- BACKSPACE -->
				<control id="23" type="button">
					<onup>18</onup>
					<width>148</width>
					<font>Material20</font>
					<textcolor>text_white</textcolor>
					<label></label>
					<include>KeyboardButtons</include>
				</control>
				<!-- Cancel Button -->
				<control id="301" type="button">
					<onup>19</onup>
					<width>148</width>
					<font>Material24</font>
					<textcolor>text_white</textcolor>
					<label></label>
					<onclick>Close</onclick>
					<include>KeyboardButtons</include>
				</control>
				<!-- Done -->
				<control id="21" type="button">
					<onup>10</onup>
					<width>148</width>
					<font>Material24</font>
					<textcolor>text_white</textcolor>
					<label></label>
					<include>KeyboardButtons</include>
				</control>
			</control>
		</control>
	</controls>
</window>