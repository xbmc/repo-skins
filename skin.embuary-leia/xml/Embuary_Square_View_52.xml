<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="Square_View_52">
		<param name="mainlabel">$VAR[MainListLabel]</param>
		<param name="sublabel">$VAR[SubListLabel]</param>
		<param name="thumblabel">$VAR[MainListLabel]</param>
		<definition>
			<control type="group">
				<include>Visible_OpenClose</include>
				<visible>Control.IsVisible(52)</visible>
				<animation effect="slide" condition="![Integer.IsGreaterOrEqual(Container(52).CurrentItem,13) | Container(52).HasPrevious]" time="200" end="0,-78">Conditional</animation>
				<control id="52" type="panel">
					<visible>$EXP[SquareViewVisible]</visible>
					<include>FocusBounce</include>
					<width>1920</width>
					<height>1196</height>
					<onright condition="Control.IsVisible(7000)">SetFocus(7000)</onright>
					<onright condition="!Control.IsVisible(7000) + Control.IsVisible(17)">17</onright>
					<onright>noop</onright>
					<ondown>noop</ondown>
					<onup>SetFocus(6666)</onup>
					<onleft>900</onleft>
					<include>ScrollTime</include>
					<orientation>vertical</orientation>
					<preloaditems>4</preloaditems>
					<defaultcontrol always="true">0</defaultcontrol>
					<viewtype label="$LOCALIZE[31005]">panel</viewtype>
					<pagecontrol>17</pagecontrol>
					<itemlayout width="300" height="400">
						<control type="group">
							<left>70</left>
							<top>200</top>
							<include content="SquareLayout">
								<param name="mainlabel" value="$PARAM[mainlabel]" />
								<param name="thumblabel" value="$PARAM[thumblabel]" />
								<param name="sublabel" value="$PARAM[sublabel]" />
								<param name="focused" value="false" />
								<param name="id" value="52" />
							</include>
						</control>
					</itemlayout>
					<focusedlayout width="300" height="400">
						<include content="SetPropertyOnFocus">
							<param name="id" value="52"/>
						</include>
						<control type="group">
							<left>70</left>
							<top>200</top>
							<include content="SquareLayout">
								<param name="mainlabel" value="$PARAM[mainlabel]" />
								<param name="thumblabel" value="$PARAM[thumblabel]" />
								<param name="sublabel" value="$PARAM[sublabel]" />
								<param name="focused" value="true" />
								<param name="id" value="52" />
							</include>
						</control>
					</focusedlayout>
				</control>
			</control>
		</definition>
	</include>
</includes>