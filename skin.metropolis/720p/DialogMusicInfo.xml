			<include>Animation_FadedByMenu</include>
			<include>Animation_CommonFade</include>
			<include>Animation_OpenCloseFade</include>
			<posx>352</posx>
			<posy>555</posy>
			<width>515</width>
			<height>70</height>
			<itemgap>10</itemgap>
			<align>center</align>
			<orientation>horizontal</orientation>
			<onleft>7001</onleft>
			<onright>7001</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<!-- Play -->
			<control type="button" id="18">
				<include>VideoInfoButton</include>
				<texturenofocus>osd/osd_button_play_nf.png</texturenofocus>
				<texturefocus>osd/osd_button_play_fo.png</texturefocus>
				<include>Animation_ButtonFocus</include>
				<onclick>PlayMedia($INFO[ListItem.FileNameAndPath])</onclick>
				<visible>String.IsEqual(ListItem.DBtype,song)</visible>
			</control>
			<!-- Thumbs -->
			<control type="button" id="10">
				<include>VideoInfoButton</include>
				<texturenofocus>buttons/button_thumb.png</texturenofocus>
				<texturefocus>buttons/button_thumbFO.png</texturefocus>
				<include>Animation_ButtonFocus</include>
			</control>
			<!-- Fanart -->
			<control type="button" id="12">
				<include>VideoInfoButton</include>
				<texturenofocus>buttons/button_fanart.png</texturenofocus>
				<texturefocus>buttons/button_fanartFO.png</texturefocus>
				<include>Animation_ButtonFocus</include>
				<visible>!String.IsEqual(ListItem.DBtype,album) + !String.IsEqual(ListItem.DBtype,song)</visible>
			</control>
			<!-- Artwork Downloading Button, artwork.beef -->
			<control type="button" id="20">
				<include>VideoInfoButton</include>
				<texturenofocus>osd/osd_button_channel_previous_nf.png</texturenofocus>
				<texturefocus>osd/osd_button_channel_previous_fo.png</texturefocus>
				<onclick condition="String.IsEqual(ListItem.DBTYPE,artist)">RunScript(script.artwork.beef, mediatype=artist, dbid=$INFO[ListItem.DBID])</onclick>
				<onclick condition="String.IsEqual(ListItem.DBTYPE,album)">RunScript(script.artwork.beef, mediatype=album, dbid=$INFO[ListItem.DBID])</onclick>
				<onclick condition="String.IsEqual(ListItem.DBTYPE,song)">RunScript(script.artwork.beef, mediatype=song, dbid=$INFO[ListItem.DBID])</onclick>
				<include>Animation_ButtonFocus</include>
				<visible>System.HasAddon(script.artwork.beef) + [String.IsEqual(ListItem.DBType,artist) | String.IsEqual(ListItem.DBType,album) | String.IsEqual(ListItem.DBType,song)]</visible>
			</control>
			<!-- Set Rating -->
			<control type="button" id="7">
				<include>VideoInfoButton</include>
				<texturenofocus>buttons/button_videoextras.png</texturenofocus>
				<texturefocus>buttons/button_videoextrasFO.png</texturefocus>
				<include>Animation_ButtonFocus</include>
			</control>
		</control>
		<!-- Control Button Text -->
		<control type="group">
			<include>Animation_FadedByMenu</include>
			<include>Animation_CommonFade</include>
			<include>Animation_OpenCloseFade</include>
			<control type="label">
				<description>Play</description>
				<include>VideoInfoButtonLabel</include>
				<label>Play</label>
				<visible>ControlGroup(7001).HasFocus(18)</visible>
				<visible>String.IsEqual(ListItem.DBtype,song)</visible>
			</control>
			<control type="label">
				<description>Thumbs</description>
				<include>VideoInfoButtonLabel</include>
				<info>Control.GetLabel(10)</info>
				<visible>ControlGroup(7001).HasFocus(10)</visible>
			</control>
			<control type="label">
				<description>Fanart</description>
				<include>VideoInfoButtonLabel</include>
				<info>Control.GetLabel(12)</info>
				<visible>ControlGroup(7001).HasFocus(12)</visible>
			</control>
			<control type="label">
				<description>Refresh</description>
				<include>VideoInfoButtonLabel</include>
				<info>Control.GetLabel(6)</info>
				<visible>ControlGroup(7001).HasFocus(6)</visible>
			</control>
			<control type="label">
				<description>Set Rating</description>
				<include>VideoInfoButtonLabel</include>
				<info>Control.GetLabel(7)</info>
				<visible>ControlGroup(7001).HasFocus(7)</visible>
			</control>
			<control type="label">
				<description>Extra Artwork Downloader</description>
				<include>VideoInfoButtonLabel</include>
				<label>$INFO[System.AddonTitle(script.artwork.beef)]</label>
				<visible>ControlGroup(7001).HasFocus(20)</visible>
			</control>
		</control>
		<include>Object_InfoLines</include>
		<include>Global_ContextFilter</include>
		<include condition="Skin.HasSetting(DebugGrid)">Object_DebugGrid</include>
	</controls>
</window>
