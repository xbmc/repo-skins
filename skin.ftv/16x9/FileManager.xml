<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- filemanager -->
	<backgroundcolor>0xff12161b</backgroundcolor>
	<defaultcontrol always="true">20</defaultcontrol>
	<controls>
		<!-- Heading -->
		<control type="label">
			<left>90</left>
			<top>84</top>
			<width>auto</width>
			<height>80</height>
			<font>Med48</font>
			<aligny>bottom</aligny>
			<textcolor>ffdadada</textcolor>
			<label>7</label>
			<animation delay="400" effect="fade" end="100" start="0" time="200">WindowOpen</animation>
		</control>
		<!-- Main group -->
		<control type="group">
			<include>DialogZoomAnimation</include>
			<!-- Left group -->
			<control type="group">
				<left>90</left>
				<top>174</top>
				<width>825</width>
				<height>816</height>
				<control type="image">
					<width>825</width>
					<height>816</height>
					<texture border="1,61,1,1">dialogs/file_manager_back.png</texture>
				</control>
				<!-- Root label -->
				<control id="101" type="label">
					<left>40</left>
					<top>14</top>
					<height>30</height>
					<width>605</width>
					<aligny>center</aligny>
					<font>Bold30</font>
					<haspath>true</haspath>
					<textcolor>ffdadada</textcolor>
				</control>
				<control type="label">
					<left>645</left>
					<top>13</top>
					<width>140</width>
					<height>30</height>
					<font>Light30Spaced</font>
					<align>right</align>
					<textcolor>ff636364</textcolor>
					<label>[COLOR ffb2b2b2]$INFO[Container(20).CurrentItem][/COLOR] | $INFO[Container(20).NumItems]</label>
				</control>
				<!-- Left list -->
				<control id="20" type="fixedlist">
					<top>60</top>
					<width>825</width>
					<height>756</height>
					<onleft>Back</onleft>
					<onright>21</onright>
					<onup>20</onup>
					<ondown>20</ondown>
					<movement>4</movement>
					<scrolltime>0</scrolltime>
					<focusposition>4</focusposition>
					<orientation>vertical</orientation>
					<animation condition="String.IsEqual(Container(20).NumItems,0) | String.IsEqual(Container(20).NumItems,1) | [String.IsEqual(Container(20).NumItems,2) + !Container(20).HasParent]" effect="slide" end="0,-336">Conditional</animation>
					<animation condition="[String.IsEqual(Container(20).NumItems,2) + Container(20).HasParent] | String.IsEqual(Container(20).NumItems,3) | [String.IsEqual(Container(20).NumItems,4) + !Container(20).HasParent]" effect="slide" end="0,-252">Conditional</animation>
					<animation condition="[String.IsEqual(Container(20).NumItems,4) + Container(20).HasParent] | String.IsEqual(Container(20).NumItems,5) | [String.IsEqual(Container(20).NumItems,6) + !Container(20).HasParent]" effect="slide" end="0,-168">Conditional</animation>
					<animation condition="[String.IsEqual(Container(20).NumItems,6) + Container(20).HasParent] | String.IsEqual(Container(20).NumItems,7) | [String.IsEqual(Container(20).NumItems,8) + !Container(20).HasParent]" effect="slide" end="0,-84">Conditional</animation>
					<itemlayout height="84" width="825">
						<control type="image">
							<width>825</width>
							<height>84</height>
							<texture border="3">settings/button_no_focus.png</texture>
						</control>
						<control type="label">
							<left>40</left>
							<width>745</width>
							<height>84</height>
							<font>Light32</font>
							<textcolor>ff505153</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
						<control type="label">
							<right>40</right>
							<width>745</width>
							<height>84</height>
							<align>right</align>
							<font>Light32</font>
							<textcolor>ff717171</textcolor>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</itemlayout>
					<focusedlayout height="84" width="825">
						<control type="image">
							<width>825</width>
							<height>84</height>
							<texture border="3">settings/button_focus.png</texture>
						</control>
						<control type="group">
							<visible>Control.HasFocus(20)</visible>
							<control type="label">
								<left>40</left>
								<width>745</width>
								<height>84</height>
								<font>Light32</font>
								<textcolor>ffffaf37</textcolor>
