<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="Include_DialogSettings_Default">
		<include>DialogBackgroundImage</include>
		<!-- Main group -->
		<control type="group">
			<include>DialogAnimation</include>
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>1337</width>
			<height>480</height>
			<animation condition="Window.IsVisible(profiles) | Window.IsVisible(settingscategory.xml)" effect="slide" end="0,-11">Conditional</animation>
			<control type="image">
				<width>1337</width>
				<height>480</height>
				<texture>dialogs/network_back.png</texture>
			</control>
			<!-- Heading -->
			<control type="label" id="2">
				<left>40</left>
				<top>14</top>
				<height>30</height>
				<width>937</width>
				<aligny>center</aligny>
				<font>Bold30</font>
				<textcolor>ffdadada</textcolor>
			</control>
			<!-- Grouplist -->
			<control type="grouplist" id="5">
				<top>60</top>
				<width>937</width>
				<height>420</height>
				<itemgap>0</itemgap>
				<onleft>noop</onleft>
				<onright>9001</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<scrolltime>0</scrolltime>
				<orientation>vertical</orientation>
				<usecontrolcoords>true</usecontrolcoords>
			</control>
			<!-- Default button -->
			<control type="button" id="7">
				<width>937</width>
				<height>84</height>
				<font>Light32</font>
				<textoffsetx>40</textoffsetx>
				<textcolor>ff505153</textcolor>
				<focusedcolor>ffffaf37</focusedcolor>
				<texturefocus border="3">settings/button_focus.png</texturefocus>
				<texturenofocus border="3">settings/button_no_focus.png</texturenofocus>
			</control>
			<!-- Default radiobutton -->
			<control type="radiobutton" id="8">
				<width>937</width>
				<height>84</height>
				<font>Light32</font>
				<textwidth>738</textwidth>
				<textoffsetx>40</textoffsetx>
				<textcolor>ff505153</textcolor>
				<focusedcolor>ffffaf37</focusedcolor>
				<radiowidth>125</radiowidth>
				<radioheight>53</radioheight>
				<radioposx>773</radioposx>
				<textureradioonfocus>settings/radio_on_focus.png</textureradioonfocus>
				<textureradioonnofocus>settings/radio_on_no_focus.png</textureradioonnofocus>
				<textureradioondisabled>settings/radio_on_disabled.png</textureradioondisabled>
				<textureradioofffocus>settings/radio_off_focus.png</textureradioofffocus>
				<textureradiooffnofocus>settings/radio_off_no_focus.png</textureradiooffnofocus>
				<textureradiooffdisabled>settings/radio_off_disabled.png</textureradiooffdisabled>
				<texturefocus border="3">settings/button_focus.png</texturefocus>
				<texturenofocus border="3">settings/button_no_focus.png</texturenofocus>
			</control>
			<!-- Default spincontrolex -->
			<control type="spincontrolex" id="9">
				<width>937</width>
				<height>84</height>
				<font>Light32</font>
				<textwidth>738</textwidth>
				<textoffsetx>40</textoffsetx>
				<textcolor>ff505153</textcolor>
				<focusedcolor>ffffaf37</focusedcolor>
				<selectedcolor>ffffffff</selectedcolor>
				<texturefocus border="3">settings/button_focus.png</texturefocus>
				<texturenofocus border="3">settings/button_no_focus.png</texturenofocus>
				<aligny>center</aligny>
				<reverse>yes</reverse>
			</control>
			<control type="image" id="11">
				<top>-120</top>
				<height>120</height>
				<texture border="3">settings/button_no_focus.png</texture>
			</control>
			<!-- Default edit -->
			<control type="edit" id="12">
				<width>937</width>
				<height>84</height>
				<font>Light32</font>
				<textoffsetx>40</textoffsetx>
				<textcolor>ff505153</textcolor>
				<focusedcolor>ffffaf37</focusedcolor>
				<texturefocus border="3">settings/button_focus.png</texturefocus>
				<texturenofocus border="3">settings/button_no_focus.png</texturenofocus>
			</control>
			<!-- Default slider -->
			<control type="sliderex" id="13">
				<width>937</width>
				<height>84</height>
				<font>Light32</font>
				<textwidth>738</textwidth>
				<textoffsetx>40</textoffsetx>
				<textcolor>ff505153</textcolor>
				<focusedcolor>ffffaf37</focusedcolor>
				<texturefocus border="3">settings/button_focus.png</texturefocus>
				<texturenofocus border="3">settings/button_no_focus.png</texturenofocus>
				<sliderwidth>200</sliderwidth>
				<sliderheight>11</sliderheight>
				<texturesliderbar border="4">osd/progress_back.png</texturesliderbar>
				<textureslidernib border="4">osd/progress_cache.png</textureslidernib>
				<textureslidernibfocus border="4">osd/progress_bar.png</textureslidernibfocus>
			</control>
			<!-- Default label -->
			<control type="label" id="14">
				<width>937</width>
				<height>84</height>
				<font>Bold30</font>
				<textoffsetx>40</textoffsetx>
				<textcolor>ffdadada</textcolor>
			</control>
			<control type="grouplist" id="9001">
				<right>24</right>
				<bottom>33</bottom>
				<width>351</width>
				<height>420</height>
				<onleft>5</onleft>
				<onright>noop</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<onback>5</onback>
				<itemgap>10</itemgap>
				<align>right</align>
				<!-- Settings Button -->
				<control type="button" id="30">
					<width>351</width>
					<height>76</height>
					<aligny>top</aligny>
					<font>Med24</font>
					<textoffsetx>23</textoffsetx>
					<textoffsety>27</textoffsety>
					<visible>Control.IsEnabled(30)</visible>
				</control>
				<!-- Cancel Button -->
				<control type="button" id="29">
					<width>351</width>
					<height>76</height>
					<aligny>top</aligny>
					<font>Med24</font>
					<textoffsetx>23</textoffsetx>
					<textoffsety>27</textoffsety>
					<visible>Control.IsEnabled(29)</visible>
				</control>
				<!-- OK Button -->
				<control type="button" id="28">
					<width>351</width>
					<height>76</height>
					<aligny>top</aligny>
					<font>Med24</font>
					<textoffsetx>23</textoffsetx>
					<textoffsety>27</textoffsety>
					<visible>Control.IsEnabled(28)</visible>
				</control>
			</control>
		</control>
	</include>
</includes>
