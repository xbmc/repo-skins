						<top>329</top>
						<width>938</width>
						<height>751</height>
						<texture border="4">dialogs/notification_back.png</texture>
						<visible/>
					</control>
					<control type="group">
						<animation effect="slide" end="0,-109" start="0,0" time="0" condition="Integer.IsGreater(Container(9000).NumItems,3) + [Control.HasFocus(9000) + [Container(9000).Row(3) | Container(9000).Row(4) | Container(9000).Row(5) | Container(9000).Row(6)]]">Conditional</animation>
						<animation effect="slide" end="0,-109" start="0,0" time="0" condition="Integer.IsGreater(Container(9000).NumItems,3) + Container(9000).Row(6)">Conditional</animation>
