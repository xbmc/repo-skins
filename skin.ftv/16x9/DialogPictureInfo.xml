<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- pictureinfo -->
	<defaultcontrol always="true">5</defaultcontrol>
	<controls>
		<include>DialogBackgroundImage</include>
		<!-- Main group -->
		<control type="group">
			<include>DialogAnimation</include>
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>1337</width>
			<height>480</height>
			<control type="image">
				<width>1337</width>
				<height>480</height>
				<texture>dialogs/network_back.png</texture>
			</control>
			<!-- Heading -->
			<control type="label">
				<left>40</left>
				<top>14</top>
				<height>30</height>
				<width>1257</width>
				<aligny>center</aligny>
				<font>Bold30</font>
				<label>13406</label>
				<textcolor>ffdadada</textcolor>
			</control>
			<!-- List -->
			<control id="5" type="fixedlist">
				<top>60</top>
				<width>937</width>
				<height>420</height>
				<onleft>noop</onleft>
				<onright>9001</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<movement>2</movement>
				<scrolltime>0</scrolltime>
				<focusposition>2</focusposition>
				<orientation>vertical</orientation>
				<animation condition="String.IsEqual(Container(5).NumItems,0) | String.IsEqual(Container(5).NumItems,1) | [String.IsEqual(Container(5).NumItems,2) + !Container(5).HasParent]" effect="slide" end="0,-168">Conditional</animation>
				<animation condition="[String.IsEqual(Container(5).NumItems,2) + Container(5).HasParent] | String.IsEqual(Container(5).NumItems,3) | [String.IsEqual(Container(5).NumItems,4) + !Container(5).HasParent]" effect="slide" end="0,-84">Conditional</animation>
				<itemlayout height="84" width="937">
					<control type="image">
						<width>937</width>
						<height>84</height>
						<texture border="3">settings/button_no_focus.png</texture>
					</control>
					<control type="label">
						<left>40</left>
						<width>857</width>
						<height>84</height>
						<font>Light32</font>
						<textcolor>ff505153</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<left>40</left>
						<width>857</width>
						<height>84</height>
						<font>Light32</font>
						<align>right</align>
						<textcolor>ff505153</textcolor>
						<label>$INFO[ListItem.Label2]</label>
					</control>
				</itemlayout>
				<focusedlayout height="84" width="937">
					<control type="image">
						<width>937</width>
						<height>84</height>
						<texture border="3">settings/button_focus.png</texture>
					</control>
					<control type="label">
						<left>40</left>
						<width>857</width>
						<height>84</height>
						<font>Light32</font>
						<textcolor>ffffaf37</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<left>40</left>
						<width>857</width>
						<height>84</height>
						<font>Light32</font>
						<align>right</align>
						<textcolor>ffffaf37</textcolor>
						<label>$INFO[ListItem.Label2]</label>
					</control>
				</focusedlayout>
			</control>
			<control type="image">
				<top>60</top>
				<width>937</width>
				<height>84</height>
				<visible>Container(5).HasPrevious</visible>
				<texture border="2" diffuse="settings/mask.png" flipy="true">settings/mask_back.png</texture>
			</control>
			<control type="image">
				<top>396</top>
				<width>937</width>
				<height>84</height>
				<visible>Container(5).HasNext</visible>
				<texture border="2" diffuse="settings/mask.png">settings/mask_back.png</texture>
			</control>
			<control type="grouplist" id="9001">
				<right>24</right>
				<bottom>33</bottom>
				<width>311</width>
				<height>420</height>
				<onleft>5</onleft>
				<onright>noop</onright>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<onback>5</onback>
				<itemgap>10</itemgap>
				<align>right</align>
				<!-- OK Button -->
				<control type="button">
					<width>311</width>
					<height>76</height>
					<aligny>top</aligny>
					<onclick>Close</onclick>
					<font>Med24</font>
					<textoffsetx>23</textoffsetx>
					<textoffsety>27</textoffsety>
					<label>186</label>
				</control>
			</control>
		</control>
	</controls>
</window>