<includes>
  <!-- Common animations -->
  <include name="Window_OpenClose_Animation">
    <animation effect="fade" time="250">WindowOpen</animation>
    <animation effect="fade" time="250">WindowClose</animation>
  </include>
  <include name="Animation_CloseZoom">
    <animation type="WindowClose" reversible="false">
      <effect type="fade" start="100" end="0" time="300"/>
      <effect type="zoom" start="100" end="130" center="640,360" time="300" tween="cubic" easing="in"/>
    </animation>
  </include>
  <include name="Animation_OpenCloseSlideWidget">
    <animation type="WindowOpen" reversible="false" condition="Window.Previous(Startup)">
      <effect type="slide" start="1500,0" end="0,0" time="1000" delay="3000" tween="cubic" easing="out" />
    </animation>
    <animation type="WindowOpen" reversible="false" condition="!Window.Previous(Startup)">
      <effect type="slide" start="1500,0" end="0,0" time="500" delay="1000" tween="cubic" easing="out" />
    </animation>
    <animation type="Visible" reversible="false">
      <effect type="slide" start="1500,0" end="0,0" time="500" delay="200" tween="cubic" easing="inout" />
    </animation>
    <animation type="Hidden" reversible="false">
      <effect type="slide" start="0,0" end="1500,0" time="500" delay="200" tween="cubic" easing="inout" />
    </animation>
    <include>Animation_CloseZoom</include>
  </include>
  <include name="Animation_ZoomedByMenu">
    <animation type="Conditional" condition="ControlGroup(7000).HasFocus | Window.IsActive(100) | Window.IsActive(106) | Window.IsActive(111) | Window.IsActive(134) | Window.IsActive(2002) | Window.IsActive(103) | Window.IsActive(129)">
      <effect type="zoom" time="600" center="640,360" start="100" end="50" tween="cubic" easing="inout" />
      <effect type="fade" time="400" start="100" end="20" />
    </animation>
