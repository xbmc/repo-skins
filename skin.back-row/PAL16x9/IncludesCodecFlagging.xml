<includes>
	<include name="CodecFlaggingConditions">

		<control type="image">
			<description>Video rez Image</description>
			<width>60</width>
			<height>24.2</height>
			<aspectratio>keep</aspectratio>
			<texture>$INFO[ListItem.VideoResolution,special://skin/images/flagging/video/,.png]</texture>
		</control>

		<control type="image">
			<description>Common Codec Image</description>
			<width>60</width>
			<height>24.2</height>
			<aspectratio>keep</aspectratio>
			<texture>$INFO[ListItem.VideoCodec,special://skin/images/flagging/video/,.png]</texture>
			<!-- Don't show if its one of the 500 divx codecs -->
			<visible>![substring(ListItem.VideoCodec,div,left) | stringcompare(ListItem.VideoCodec,dx50)]</visible>
		</control>

		<control type="image">
			<description>Divx Codec Image</description>
			<width>60</width>
			<height>24.2</height>
			<aspectratio>keep</aspectratio>
			<texture>special://skin/images/flagging/video/divx.png</texture>
			<visible>[substring(ListItem.VideoCodec,div,left) | stringcompare(ListItem.VideoCodec,dx50)]</visible>
		</control>

		<control type="image">
			<description>Audio Codec Image</description>
			<width>60</width>
			<height>24.2</height>
			<aspectratio>keep</aspectratio>
			<texture>$INFO[ListItem.AudioCodec,special://skin/images/flagging/audio/,.png]</texture>
			<!-- Don't show if I want the Dolby Digital Logo -->
			<visible>![stringcompare(ListItem.AudioCodec,ac3) + stringcompare(ListItem.AudioChannels,6)]</visible>
		</control>

		<control type="image">
			<description>Dolby Digital Image</description>
			<width>60</width>
			<height>24.2</height>
			<aspectratio>keep</aspectratio>
			<texture>special://skin/images/flagging/audio/dolbydigital.png</texture>
			<visible>[stringcompare(ListItem.AudioCodec,ac3) + stringcompare(ListItem.AudioChannels,6)]</visible>
		</control>

		<control type="image">
			<description>Audio Channels Image</description>
			<width>60</width>
			<height>24.2</height>
			<aspectratio>keep</aspectratio>
			<texture>$INFO[ListItem.AudioChannels,special://skin/images/flagging/audio/,.png]</texture>
		</control>

		<control type="image">
			<description>Aspectratio Image</description>
			<width>60</width>
			<height>24.2</height>
			<aspectratio>keep</aspectratio>
			<texture>$INFO[ListItem.VideoAspect,special://skin/images/flagging/aspectratio/,.png]</texture>
		</control>

	</include>

