			<visible>substring(ListItem.FilenameAndPath,vhs)</visible>
		</control>

	</include>

	<include name="recordlabel">

			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>40</width>
				<height>38</height>
				<aspectratio>keep</aspectratio>
				<align>right</align>
				<aligny>center</aligny>
				<texture fallback="disc.png">special://skin/images/record labels/$INFO[ListItem.Property(Album_Label),,.png]</texture>
				<visible>Container.Content(Albums) + !stringcompare(ListItem.Label,..)</visible>
				<animation effect="fade" time="200">WindowClose</animation>
			</control>

	</include>

