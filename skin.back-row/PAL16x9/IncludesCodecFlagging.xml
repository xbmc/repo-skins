	<include name="VideoTypeHackFlaggingConditions">

		<control type="image">
			<description>Bluray Image</description>
			<width>70</width>
			<height>40</height>
			<aspectratio align="right">keep</aspectratio>
			<texture>special://skin/images/flagging/video/bluray.png</texture>
			<visible>[substring(ListItem.FilenameAndPath,bluray) | substring(ListItem.FilenameAndPath,bdrip)]</visible>
		</control>

		<control type="image">
			<description>HDDVD Image</description>
			<width>70</width>
			<height>40</height>
			<aspectratio align="right">keep</aspectratio>
			<texture>special://skin/images/flagging/video/hddvd.png</texture>
			<visible>substring(ListItem.FilenameAndPath,hddvd)</visible>
		</control>

		<control type="image">
			<description>DVD Image</description>
			<width>70</width>
			<height>40</height>
			<aspectratio align="right">keep</aspectratio>
			<texture>special://skin/images/flagging/video/dvd.png</texture>
			<visible>[substring(ListItem.FilenameAndPath,dvd) + ![substring(ListItem.FilenameAndPath,hddvd) | substring(ListItem.FilenameAndPath,bluray) | substring(ListItem.FilenameAndPath,bdrip)]]</visible>
		</control>

		<control type="image">
			<description>PDTV Image</description>
			<width>70</width>
			<height>40</height>
			<aspectratio align="right">keep</aspectratio>
			<texture>special://skin/images/flagging/video/tv.png</texture>
			<visible>substring(listitem.filename,pdtv) | substring(ListItem.FilenameAndPath,sdtv)</visible>
		</control>

		<control type="image">
			<description>HDTV Image</description>
			<width>70</width>
			<height>40</height>
			<aspectratio align="right">keep</aspectratio>
			<texture>special://skin/images/flagging/video/hdtv.png</texture>
			<colordiffuse>CCFFFFFF</colordiffuse>
			<visible>substring(listitem.filename,hdtv)</visible>
		</control>

		<control type="image">
			<description>WEB-DL Image</description>
			<width>70</width>
			<height>40</height>
			<aspectratio align="right">keep</aspectratio>
			<texture>special://skin/images/flagging/video/web-dl.png</texture>
			<colordiffuse>CCFFFFFF</colordiffuse>
			<visible>substring(listitem.filename,web-dl)</visible>
