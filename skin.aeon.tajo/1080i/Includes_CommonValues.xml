<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<!-- ######## -->
	<!-- Defaults -->
	<!-- ######## -->
	<include name="FullscreenDimensions">
		<width>100%</width>
		<height>100%</height>
	</include>
	<include name="HiddenObject">
		<left>-300</left>
		<top>-300</top>
		<width>1</width>
		<height>1</height>
		<texturefocus/>
		<texturenofocus/>
		<label/>
		<font/>
	</include>
	<include name="HiddenList">
		<left>-100</left>
		<top>-100</top>
		<width>100</width>
		<height>100</height>
		<itemlayout height="100" width="100"/>
		<focusedlayout height="100" width="100"/>
	</include>
	<!-- Buttons -->
	<include name="ButtonTopbarCommonvalues">
		<height>60</height>
		<width>60</width>
		<align>center</align>
		<aligny>top</aligny>
		<textoffsety>3</textoffsety>
		<font>IconSmall</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<texturenofocus/>
		<shadowcolor>black</shadowcolor>
		<alttexturenofocus/>
	</include>
	<include name="ButtonCommonValues">
		<width>400</width>
		<height>60</height>
		<textoffsetx>15</textoffsetx>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<textwidth>300</textwidth>
		<font>font13</font>
		<radioleft>340</radioleft>
	</include>
	<include name="ButtonInfoDialogsCommonValues">
		<height>60</height>
		<width>234</width>
		<textoffsetx>0</textoffsetx>
		<align>center</align>
		<font>font15</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
	</include>
	<include name="SettingsButtonCommonValues">
		<width>1201</width>
		<height>81</height>
		<textoffsetx>30</textoffsetx>
		<font>font15</font>
		<radioleft>1281</radioleft>
	</include>
	<!-- KeyBoard -->
	<include name="Objects_KeyOffVars">
		<textoffsety>32</textoffsety>
		<align>center</align>
		<font>font15</font>
		<selectedcolor>grey2</selectedcolor>
	</include>
	<include name="Objects_DoubleKeyOff">
		<left>-123</left>
		<width>320</width>
		<height>80</height>
		<texturefocus colordiffuse="$INFO[Skin.String(flat_back)]" border="4">listselect_fo.png</texturefocus>
		<alttexturefocus colordiffuse="$INFO[Skin.String(flat_back)]" border="4">listselect_fo.png</alttexturefocus>
		<include>Objects_KeyOffVars</include>
	</include>
	<include name="Objects_SingleKeyOff">
		<width>80</width>
		<height>80</height>
		<texturefocus colordiffuse="$INFO[Skin.String(flat_back)]" border="4">listselect_fo.png</texturefocus>
		<include>Objects_KeyOffVars</include>
	</include>
	<include name="ShowCaseInfoPanelButtonsValues">
		<height>90</height>
		<width>501</width>
		<textoffsetx>15</textoffsetx>
		<aligny>top</aligny>
		<font>font14</font>
		<textcolor>grey2</textcolor>
		<focusedcolor>grey2</focusedcolor>
		<onclick>noop</onclick>
	</include>
	<include name="SettingsPanelButtonsValues">
		<height>90</height>
		<width>1256</width>
		<textoffsetx>15</textoffsetx>
		<textoffsety>-4</textoffsety>
		<aligny>top</aligny>
		<font>font15</font>
		<textcolor>grey2</textcolor>
		<focusedcolor>grey2</focusedcolor>
		<onclick>noop</onclick>
	</include>
	<!-- Media Flags -->
	<include name="MediaFlagVars">
		<width>135</width>
		<height>90</height>
		<aspectratio>keep</aspectratio>
		<fadetime>250</fadetime>
		<include condition="!Skin.HasSetting(Coloured.icons)">IconGrey</include>
		<animation effect="fade" time="200">VisibleChange</animation>
	</include>
</includes>