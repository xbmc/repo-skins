<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="image">
		<texture />
	</default>
	<default type="multiimage">
		<aspectratio align="center">scale</aspectratio>
		<fadetime>FanartCrossfadeTime</fadetime>
		<include>FullscreenDimensions</include>
		<randomize>true</randomize>
	</default>
	<default type="label">
		<align>left</align>
		<aligny>center</aligny>
		<label />
		<font>font13</font>
		<textcolor>white</textcolor>
		<selectedcolor>$VAR[ThemeLabelColor]</selectedcolor>
		<disabledcolor>grey3</disabledcolor>
		<scrollspeed>30</scrollspeed>
	</default>
	<default type="fadelabel">
		<font>font13</font>
		<textcolor>white</textcolor>
		<disabledcolor>grey3</disabledcolor>
		<scrollspeed>40</scrollspeed>
	</default>
	<default type="button">
		<width>450</width>
		<height>60</height>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</texturefocus>
		<font>font14</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<textoffsetx>15</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="togglebutton">
		<width>30</width>
		<height>60</height>
		<textoffsetx>15</textoffsetx>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</texturefocus>
		<alttexturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</alttexturefocus>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<label />
		<aligny>center</aligny>
		<font>font14</font>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="textbox">
		<width>615</width>
		<height>322</height>
		<align>left</align>
		<textcolor>white</textcolor>
		<font>font15_textbox</font>
	</default>
	<default type="progress">
		<lefttexture />
		<righttexture />
		<overlaytexture />
		<midtexture border="2">osd/midtexture.png</midtexture>
		<texturebg border="1">osd/texturebg_cache.png</texturebg>
	</default>
	<default type="spincontrol">
		<textureup colordiffuse="grey">android/numberpicker_up_normal_holo_dark.png</textureup>
		<texturedown colordiffuse="grey">android/numberpicker_down_normal_holo_dark.png</texturedown>
		<textureupfocus colordiffuse="$VAR[HighlightBarColor]">android/numberpicker_up_normal_holo_dark.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[HighlightBarColor]">android/numberpicker_down_normal_holo_dark.png</texturedownfocus>
		<texturedowndisabled colordiffuse="40FFFFFF">android/numberpicker_down_normal_holo_dark.png</texturedowndisabled>
		<textureupdisabled colordiffuse="40FFFFFF">android/numberpicker_up_normal_holo_dark.png</textureupdisabled>
		<align>right</align>
		<width>40</width>
		<height>25</height>
		<font>font14</font>
		<reverse>yes</reverse>
		<textcolor>grey</textcolor>
		<disabledcolor>grey3</disabledcolor>
		<subtype>text</subtype>
		<textoffsetx>0</textoffsetx>
		<textoffsety>0</textoffsety>
		<aligny>center</aligny>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrolex">
		<width>450</width>
		<height>60</height>
		<spinwidth>40</spinwidth>
		<spinheight>25</spinheight>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</texturefocus>
		<textureup colordiffuse="grey">android/numberpicker_up_normal_holo_dark.png</textureup>
		<texturedown colordiffuse="grey">android/numberpicker_down_normal_holo_dark.png</texturedown>
		<textureupfocus colordiffuse="$VAR[HighlightBarColor]">android/numberpicker_up_normal_holo_dark.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[HighlightBarColor]">android/numberpicker_down_normal_holo_dark.png</texturedownfocus>
		<texturedowndisabled colordiffuse="40FFFFFF">android/numberpicker_down_normal_holo_dark.png</texturedowndisabled>
		<textureupdisabled colordiffuse="40FFFFFF">android/numberpicker_up_normal_holo_dark.png</textureupdisabled>
		<font>font14</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<textoffsetx>15</textoffsetx>
		<aligny>center</aligny>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="radiobutton">
		<width>450</width>
		<height>60</height>
		<radiowidth>120</radiowidth>
		<radioheight>76</radioheight>
		<textureradioonfocus colordiffuse="$VAR[ThemeLabelColor]">android/btn_radio_on_holo_dark.png</textureradioonfocus>
		<textureradioonnofocus colordiffuse="$VAR[ThemeLabelColor]">android/btn_radio_on_holo_dark.png</textureradioonnofocus>
		<textureradioofffocus colordiffuse="grey">android/btn_radio_off_holo_dark.png</textureradioofffocus>
		<textureradiooffnofocus colordiffuse="grey">android/btn_radio_off_holo_dark.png</textureradiooffnofocus>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</texturefocus>
		<textureradioondisabled colordiffuse="40FFFFFF">android/btn_radio_on_holo_dark.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="40FFFFFF">android/btn_radio_off_holo_dark.png</textureradiooffdisabled>
		<font>font14</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<textoffsetx>15</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="slider">
		<orientation>horizontal</orientation>
	</default>
	<default type="sliderex">
		<height>60</height>
		<textoffsetx>15</textoffsetx>
		<aligny>center</aligny>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</texturefocus>
		<sliderwidth>150</sliderwidth>
		<sliderheight>20</sliderheight>
		<texturesliderbar>osd_slider_bg.png</texturesliderbar>
		<textureslidernib>osd_slider_nibNF.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[ThemeLabelColor]">osd_slider_nib.png</textureslidernibfocus>
		<font>font14</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<pulseonselect>false</pulseonselect>
		<scrollspeed>30</scrollspeed>
	</default>
	<default type="mover">
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="resize">
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="edit">
		<width>450</width>
		<height>60</height>
		<texturefocus colordiffuse="$VAR[HighlightBarColor]" border="1">listselect_fo.png</texturefocus>
		<font>font14</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>grey3</disabledcolor>
		<invalidcolor>invalid</invalidcolor>
		<textoffsetx>15</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="grouplist">
		<itemgap>0</itemgap>
	</default>
</includes>