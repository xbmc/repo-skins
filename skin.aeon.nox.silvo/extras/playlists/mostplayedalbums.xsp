<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="albums">
	<name>Most Played Albums</name>
	<match>all</match>
	<limit>15</limit>
	<order direction="descending">playcount</order>
	<rule field="playcount" operator="greaterthan">
		<value>0</value>
	</rule>
</smartplaylist>
