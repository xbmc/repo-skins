<?xml version="1.0" encoding="utf-8"?>
<includes>
    <include name="Viewtype_Panel_Year">
        <control type="group">
            <visible>Control.IsVisible(531)</visible>
            <animation type="TYPE" condition="Window.IsVisible(2003)" reversible="false">
                <effect type="slide" end="-1500,0" time="200" />
            </animation>
            <animation type="TYPE" condition="!Window.IsVisible(2003)" reversible="false">
                <effect type="slide" start="-1500,0" time="200" />
            </animation>
            <animation type="WindowClose">
                <effect type="fade" time="300" />
                <effect type="slide" end="+1500,0" time="300" />
            </animation>
            <animation type="WindowOpen">
                <effect type="fade" time="300" />
                <effect type="slide" start="+1500,0" time="300" />
            </animation>
            <animation effect="fade" time="300">WindowOpen</animation>
            <animation type="WindowClose">
                <effect type="fade" time="300" />
            </animation>
            <control type="panel" id="531">
                <viewtype label="Panel">Years</viewtype>
                <left>50</left>
                <top>290</top>
                <width>1200</width>
                <height>360</height>
                <scrolltime>200</scrolltime>
                <orientation>vertical</orientation>
                <onright>531</onright>
				 <ondown>noop</ondown>
				 <onup>noop</onup>
                <onleft>9000</onleft>
				<onleft>Skin.ToggleSetting(showViewOptions)</onleft>
                <pagecontrol>62</pagecontrol>
                <preloaditems>2</preloaditems>
                <visible>Container.Content(years)</visible>
                <itemlayout width="120" height="120">
                    
					<control type="image">
						<width>115</width>
						<height>115</height>
						<texture>tile.png</texture>
					</control>
					<control type="image">
						<left>20</left>
						<top>20</top>
						<width>75</width>
						<height>75</height>
						<texture>icons/calendar.png</texture>
					</control>
					<control type="label">
						<top>7</top>
						<width>115</width>
						<height>115</height>
						<align>center</align>
						<aligny>center</aligny>
                        <label>$INFO[ListItem.Label]</label>
						<font>Panel_Hint</font>
                     </control>
				</itemlayout>
                <focusedlayout width="120" height="120">
                    <control type="image">
						<width>115</width>
						<height>115</height>
						<texture>button.png</texture>
						<animation type="Focus" reversible="false">
							<effect type="fade" start="0" end="100" time="400" easing="out" tween="linear" />
						</animation>
					</control>
					<control type="image">
						<left>20</left>
						<top>20</top>
						<width>75</width>
						<height>75</height>
						<texture>icons/calendar.png</texture>
					</control>
                    <control type="label">
						<top>7</top>
						<width>115</width>
						<height>115</height>
						<align>center</align>
						<aligny>center</aligny>
                        <label>$INFO[ListItem.Label]</label>
						<font>Panel_Hint</font>
                     </control>
                </focusedlayout>
            </control>
            <control type="label">
				<include>ViewMainTitle</include>
                <label>652</label>
            </control>
         </control>
    </include>
</includes>