<?xml version="1.0" encoding="utf-8"?>
<includes>
    <!-- This defines the default settings of any label -->
	<default type="label">
        <height>45</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <selectedcolor>Accent</selectedcolor>
        <disabledcolor>Red</disabledcolor>
	</default>
	<default type="title">
        <left>0</left>
        <top>0</top>
     </default>
    <!-- This defines the default settings of a fading label -->
	<default type="fadelabel">
        <left>0</left>
        <top>0</top>
        <height>80</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <selectedcolor>Accent</selectedcolor>
        <disabledcolor>Red</disabledcolor>
        <scrollout>false</scrollout>
        <pauseatend>2000</pauseatend>
    </default>
	<!-- This defines the default settings of an Image -->
    <default type="image">
        <left>0</left>
        <top>0</top>
        <width>-</width>
        <height>-</height>
        <align>center</align>
        <aligny>center</aligny>
        <texture>-</texture>
    </default>
	<!-- This defines the default settings of a Button -->
    <default type="button">
        <left>0</left>
        <top>0</top>
        <width>300</width>
        <height>45</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textoffsetx>20</textoffsetx>
        <textcolor>White</textcolor>
        <focusedcolor>Accent</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <texturefocus>tile.png</texturefocus>
        <texturenofocus>other_textures/buttonnf.png</texturenofocus>
        <pulseonselect>false</pulseonselect>
    </default>
	<!-- This defines the default settings of a Toggle -->
    <default type="togglebutton">
        <left>0</left>
        <top>0</top>
        <width>300</width>
        <height>45</height>
        <textoffsetx>20</textoffsetx>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>Accent</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <texturenofocus border="2">other_textures/buttonnf.png</texturenofocus>
        <texturefocus border="5">other_textures/buttonfo.png</texturefocus>
        <alttexturenofocus border="2">other_textures/buttonnf.png</alttexturenofocus>
        <alttexturefocus border="5">other_textures/buttonfo.png</alttexturefocus>
        <align>center</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
	<!-- This defines the default settings of a radio button -->
    <default type="radiobutton">
        <left>0</left>
        <top>0</top>
        <width>300</width>
        <height>45</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>Accent</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <radiowidth>30</radiowidth>
        <radioheight>30</radioheight>
        <texturenofocus>other_textures/buttonnf.png</texturenofocus>
        <texturefocus>other_textures/buttonfo.png</texturefocus>
		<textureradioonfocus>other_textures/radiobutton2fo.png</textureradioonfocus>
		<textureradioonnofocus>other_textures/radiobutton2fo.png</textureradioonnofocus>
		<textureradioofffocus>other_textures/radiobutton2nf.png</textureradioofffocus>
		<textureradiooffnofocus>other_textures/radiobutton2nf.png</textureradiooffnofocus>
		<textureradioondisabled>other_textures/radiobuttonfo.png</textureradioondisabled>
		<textureradiooffdisabled>other_textures/radiobuttonnf.png</textureradiooffdisabled>
		
        <textoffsetx>20</textoffsetx>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrolex">
        <left>0</left>
        <top>0</top>
        <width>300</width>
        <height>45</height>
        <font>Small</font>
        <spinwidth>26</spinwidth>
        <spinheight>26</spinheight>
        <texturenofocus border="2">other_textures/buttonnf.png</texturenofocus>
        <texturefocus border="5">other_textures/buttonfo.png</texturefocus>
        <textureup>other_textures/arrowupfo.png</textureup>
        <texturedown>other_textures/arrowdownfo.png</texturedown>
        <textureupfocus>other_textures/arrowupg.png</textureupfocus>
        <texturedownfocus>other_textures/arrowdowng.png</texturedownfocus>
		<textureupdisabled>other_textures/arrowupnf.png</textureupdisabled>
		<texturedowndisabled>other_textures/arrowdownnf.png</texturedowndisabled>
		<textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <left>0</left>
        <top>0</top>
        <height>15</height>
        <width>15</width>
        <spinwidth>26</spinwidth>
        <spinheight>26</spinheight>
        <textureup>other_textures/arrowupnf.png</textureup>
        <texturedown>other_textures/arrowdownnf.png</texturedown>
        <textureupfocus>other_textures/arrowupfo.png</textureupfocus>
        <texturedownfocus>other_textures/arrowdownfo.png</texturedownfocus>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="sliderex">
        <left>0</left>
        <top>0</top>
        <height>45</height>
        <width>300</width>
		<orientation>horizontal</orientation> 
        <texturenofocus>other_textures/buttonnf.png</texturenofocus>
        <texturefocus>other_textures/buttonfo.png</texturefocus>
        <sliderwidth>150</sliderwidth>
        <sliderheight>15</sliderheight>
        <texturesliderbar>other_textures/osd/slider_bg.png</texturesliderbar>
        <textureslidernib>other_textures/osd/slidernibnf.png</textureslidernib>
        <textureslidernibfocus>other_textures/osd/slidernibfo.png</textureslidernibfocus>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>Accent</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <textoffsety>10</textoffsety>
    </default>
    <default type="progress">
        <left>150</left>
        <top>325</top>
        <texturebg border="6">other_textures/osd/progressback.png</texturebg>
        <lefttexture>-</lefttexture>
        <midtexture border="6">other_textures/osd/progressmid.png</midtexture>
        <righttexture>-</righttexture>
        <overlaytexture>-</overlaytexture>
    </default>
    <default type="slider">
        <left>0</left>
        <top>0</top>
        <width>420</width>
        <height>15</height>
		<orientation>horizontal</orientation> 
        <texturefocus>-</texturefocus>
        <texturenofocus>-</texturenofocus>
        <texturesliderbar>other_textures/osd/slider_bg_2.png</texturesliderbar>
        <textureslidernib border="2">other_textures/osd/slidernibnf.png</textureslidernib>
        <textureslidernibfocus border="2">other_textures/osd/slidernibfo.png</textureslidernibfocus>
    </default>
    <default type="scrollbar">
        <texturesliderbackground></texturesliderbackground>
		<textureslidernib>bar.png</textureslidernib>
		<textureslidernibfocus>other_textures\radiobutton2fo.png</textureslidernibfocus>
		<texturesliderbar></texturesliderbar>
		<texturesliderbarfocus></texturesliderbarfocus>
	    <orientation>vertical</orientation>
    </default>
    <default type="textbox">
        <font>Small</font>
        <textcolor>white</textcolor>
        <pagecontrol>61</pagecontrol>
        <autoscroll delay="6000" time="2000" repeat="10000" />
    </default>
</includes>