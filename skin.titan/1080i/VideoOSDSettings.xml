<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="123">
    <defaultcontrol always="true">5</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <controls>
        <control type="group" id="1230">
            <visible>!Window.IsActive(filebrowser) + !Window.IsActive(DialogAudioDSPSettings.xml)</visible>
            <visible>!Window.IsActive(sliderdialog)</visible>
            <include>animation_window_open_close</include>

			<control type="image">
                <!--Panel-->
               <posx>235</posx>
                <posy>90</posy>
                <width>1500</width>
                <height>650</height>
                <texture border="15">diffuse/bgpanel.png</texture>
				
				<colordiffuse>$INFO[Skin.String(GeneralPanelsColor)]</colordiffuse>
            </control>
			<control type="image">
                <!--Panel-->
                <posx>235</posx>
                <posy>90</posy>
                <width>1500</width>
                <height>650</height>
				<bordersize>10</bordersize>
                <texture border="15">diffuse/bgpanel.png</texture>
				
				<colordiffuse>$INFO[Skin.String(GeneralPanelsColor)]</colordiffuse>
            </control>

            <!--Buttons-->
            <control type="group" id="1231">
                <posx>250</posx>
                <posy>100</posy>
				
				<control type="label">
					<description>header label</description>
					<posx>105</posx>
                    <posy>48</posy>
                    <width>870</width>
                    <font>Light40</font>
                    <textoffsetx>0</textoffsetx>
                    <align>left</align>
					<label>$LOCALIZE[3] - $LOCALIZE[5]</label>
					<visible>Window.IsVisible(osdvideosettings)</visible>
				</control>
				<control type="label">
					<description>header label</description>
					<posx>105</posx>
                    <posy>48</posy>
                    <width>870</width>
                    <font>Light40</font>
                    <textoffsetx>0</textoffsetx>
                    <align>left</align>
					<label>$LOCALIZE[292] - $LOCALIZE[5]</label>
					<visible>Window.IsVisible(osdaudiosettings)</visible>
				</control>
			
                <!-- close dialog -->
                <control type="group">
                    <include>animation_fade_visible_hidden</include>
                    <posx>1380</posx>
					<posy>48</posy>
                    <include condition="Skin.HasSetting(EnableTouchSupport)">close_button</include>
                </control>
                <control type="grouplist" id="5">
                    <description>Control Area</description>
                    <posx>55</posx>
                    <posy>130</posy>
                    <width>1375</width>
                    <height>420</height>
                    <itemgap>0</itemgap>
                    <orientation>vertical</orientation>
                    <pagecontrol>60</pagecontrol>
                    <onleft>28</onleft>
                    <onright>60</onright>
                    <onup>28</onup>
                    <ondown>28</ondown>
                </control>
				<control type="scrollbar" id="60"> 
					<left>1440</left>
					<top>130</top>
					<height>420</height>
					<onleft>14000</onleft>
					<showonepage>false</showonepage>
					<visible allowhiddenfocus="true">Control.HasFocus(60) | Skin.HasSetting(EnableTouchSupport)</visible>
				</control>
                <control type="button" id="7">
                    <align>left</align>
                    <include>DialogButton</include>
                </control>
                <control type="radiobutton" id="8">
                    <align>left</align>
                    <radioposx>1220</radioposx>
                    <include>DialogButton</include>
                </control>
                <control type="spincontrolex" id="9">
                    <align>left</align>
                    <reverse>yes</reverse>
                    <include>DialogButton</include>
                </control>
				<control type="sliderex" id="13">
                    <align>left</align>
                    <include>DialogButton</include>
                </control>
            </control>
        </control>
    </controls>
</window>