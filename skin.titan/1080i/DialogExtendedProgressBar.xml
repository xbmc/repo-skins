<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="133">
    <defaultcontrol always="true">401</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <controls>
        <control type="group">
            <posx>60</posx>
            <posy>40</posy>
            <include>animation_static_dialog</include>
            <include>animation_fade_visible_hidden</include>
            <include>animation_fade_open_close</include>
            <visible>!Window.IsActive(DialogKayToast.xml)</visible>
            <control type="multiimage">
                <!--Busy Image-->
                <description>Busy animation</description>
                <posx>0</posx>
                <posy>15</posy>
                <width>64</width>
                <height>64</height>
                <imagepath>dialogs/extended_progress_bar</imagepath>
                <timeperimage>100</timeperimage>
                <fadetime>0</fadetime>
            </control>
            <control type="label" id="30">
                <!--Header-->
                <posx>90</posx>
                <posy>0</posy>
                <width>900</width>
                <height>50</height>
                <font>Bold38</font>
                <textcolor>ccmainblue</textcolor>
                <label>-</label>
                <scroll>false</scroll>
            </control>
            <control type="label" id="31">
                <!--Current Directory Label-->
                <posx>90</posx>
                <posy>40</posy>
                <width>900</width>
                <height>50</height>
                <font>Reg30</font>
                <textcolor>eewhite</textcolor>
                <label>-</label>
                <scroll>false</scroll>
            </control>
        </control>
    </controls>
</window>