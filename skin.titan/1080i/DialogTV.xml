                        <texture colordiffuse="$INFO[Skin.String(ButtonColor)]" border="5">diffuse/panel_trans.png</texture>
                    </control>
                    <control type="label">
                        <posx>50</posx>
                        <width>600</width>
                        <height>60</height>
                        <textcolor>midgrey</textcolor>
                        <info>ListItem.Label</info>
                    </control>
                    <control type="label">
                        <posx>600</posx>
                        <width>600</width>
                        <height>60</height>
                        <align>right</align>
                        <textcolor>midgrey</textcolor>
                        <info>ListItem.Label2</info>
                    </control>
                </itemlayout>
                <focusedlayout height="60">
                    <control type="image">
                        <posx>0</posx>
                        <posy>0</posy>
                        <height>60</height>
                        <width>600</width>
                        <texture diffuse="diffuse/panel.png" border="5" colordiffuse="$INFO[Skin.String(ButtonFocusColor)]">colors/color_white.png</texture>
                    </control>
                    <control type="label">
                        <posx>50</posx>
                        <width>600</width>
                        <height>60</height>
                        <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
