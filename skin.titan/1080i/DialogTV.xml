                <onleft>450</onleft>
                <onright>450</onright>
                <onup>450</onup>
                <ondown>450</ondown>
                <itemlayout height="60">
                    <control type="image">
                        <posx>0</posx>
                        <posy>0</posy>
                        <height>60</height>
