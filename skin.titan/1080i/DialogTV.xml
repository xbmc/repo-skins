                    </control>
                    <control type="label">
                        <posx>600</posx>
                        <width>600</width>
                        <height>60</height>
                        <align>right</align>
                        <textcolor>$INFO[Skin.String(GeneralTextColor)]</textcolor>
                        <info>ListItem.Label2</info>
                    </control>
                </focusedlayout>
            </control>
            <control type="image">
                <description>Large Icon Image</description>
                <posx>705</posx>
                <posy>327</posy>
                <width>300</width>
                <height>450</height>
                <texture>$INFO[Container(450).ListItem.Icon]</texture>
            </control>
