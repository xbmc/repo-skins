<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="3">
    <defaultcontrol always="true">20</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <controls>
        <!--Globalbackground-->
        <include>GlobalBackground</include>
        <!--Time & Weather-->
        <include>TimeInfo</include>
        <!--Header-->
        <include>Header</include>
        <!--Panel-->
        <control type="image">
            <posx>0</posx>
            <posy>180</posy>
            <width>1903</width>
            <height>790</height>
            <colordiffuse>aaFFFFFF</colordiffuse>
            <texture>views/filemanager_panel.png</texture>
            <include>animation_list</include>
            <visible>!Skin.HasSetting(NoFooterBar)</visible>
        </control>
        <control type="image">
            <posx>0</posx>
            <posy>180</posy>
            <width>1903</width>
            <height>790</height>
            <colordiffuse>aaFFFFFF</colordiffuse>
            <texture>views/filemanager_panel1.png</texture>
            <include>animation_list</include>
            <visible>Skin.HasSetting(NoFooterBar)</visible>
        </control>
        <control type="image">
            <posx>0</posx>
            <posy>250</posy>
            <width>1893</width>
            <height>646</height>
            <colordiffuse>99FFFFFF</colordiffuse>
            <texture diffuse="views/filemanager_matte.png">special://skin/extras/backgrounds/filemanager.png</texture>
            <aspectratio scalediffuse="false" align="center">scale</aspectratio>
            <include>animation_list</include>
        </control>
        <control type="image">
            <posx>0</posx>
            <posy>250</posy>
            <width>1893</width>
            <height>646</height>
            <texture>views/filemanager_overlay.png</texture>
            <include>animation_list</include>
        </control>
        <control type="group">
            <!--Start Left Panel-->
            <description>Left panel</description>
            <posx>0</posx>
            <posy>300</posy>
            <include>animation_list</include>
            <control type="label" id="101">
                <!--Header-->
                <description>Header Label</description>
                <posx>150</posx>
                <width>700</width>
                <align>left</align>
                <label>-</label>
                <textcolor>midgrey</textcolor>
                <font>Bold38</font>
            </control>
            <control type="list" id="20">
                <!--List 20-->
                <description>Left File List</description>
                <posx>0</posx>
                <posy>100</posy>
                <width>950</width>
                <height>420</height>
                <onleft>21</onleft>
                <onright>21</onright>
                <onup>20</onup>
                <ondown>20</ondown>
                <itemlayout height="69" width="900">
                    <control type="image">
                        <posx>130</posx>
                        <posy>7</posy>
                        <width>64</width>
                        <height>53</height>
                        <texture>common/folder.png</texture>
                        <colordiffuse>FFffffff</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>210</posx>
                        <width>480</width>
                        <align>left</align>
                        <font>Reg30</font>
                        <info>ListItem.Label</info>
                        <textcolor>midgrey</textcolor>
                    </control>
                    <control type="label">
                        <posx>890</posx>
                        <width>200</width>
                        <align>right</align>
                        <font>Reg30</font>
                        <info>ListItem.Label2</info>
                        <textcolor>midgrey</textcolor>
                    </control>
                </itemlayout>
                <focusedlayout height="69" width="900">
                    <control type="image">
                        <visible>Control.HasFocus(20)</visible>
                        <posx>100</posx>
                        <posy>7</posy>
                        <width>850</width>
                        <height>50</height>
                        <texture>common/slider.png</texture>
                        <colordiffuse>DDffffff</colordiffuse>
                    </control>
                    <control type="image">
                        <posx>130</posx>
                        <posy>7</posy>
                        <width>64</width>
                        <height>53</height>
                        <texture>common/folder.png</texture>
                        <colordiffuse>FFffffff</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>210</posx>
                        <width>480</width>
                        <align>left</align>
                        <font>Reg30</font>
                        <info>ListItem.Label</info>
                        <scrollspeed>30</scrollspeed>
                        <textcolor>mainblue</textcolor>
                    </control>
                    <control type="label">
                        <posx>890</posx>
                        <width>200</width>
                        <align>right</align>
                        <font>Reg30</font>
                        <info>ListItem.Label2</info>
                        <scrollspeed>30</scrollspeed>
                        <textcolor>mainblue</textcolor>
                    </control>
                </focusedlayout>
            </control>
            <control type="label" id="12">
