<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="PhotoWall">
		<control type="panel" id="58">
			<posx>136</posx>
			<posy>143</posy>
			<width>1008</width>
			<height>432</height>
			<onleft>9000</onleft>
			<onright>1358</onright>
			<onup>58</onup>
			<ondown>58</ondown>
			<viewtype label="31944">Wall</viewtype>
			<animation effect="rotatey" start="-15" end="-15" time="0" condition="Skin.HasSetting(tilted-videowall)">Conditional</animation>
			<scrolltime>200</scrolltime>
			<preloaditems>2</preloaditems>
			<pagecontrol>1358</pagecontrol>
			<itemlayout height="144" width="144">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>144</width>
					<height>144</height>
					<texture>-</texture>
				</control>
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<texture background="true" diffuse="mask-squ.png">$INFO[ListItem.Icon]</texture>
					<aspectratio>keep</aspectratio>
					<bordertexture border="10">picturepanel.png</bordertexture>
					<bordersize>10</bordersize>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<aspectratio>keep</aspectratio>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<visible>SubString(ListItem.Icon,Default,Left)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="144" width="144">
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<texture background="true" diffuse="mask-squ.png">$INFO[ListItem.Icon]</texture>
					<aspectratio>keep</aspectratio>
					<bordertexture border="10">picturepanel.png</bordertexture>
					<bordersize>10</bordersize>
					<animation effect="zoom" start="90" end="150" center="auto" time="600" tween="back" easing="out" reversible="false">focus</animation>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<aspectratio>keep</aspectratio>
					<animation effect="zoom" start="90" end="150" center="auto" time="600" tween="back" easing="out" reversible="false">focus</animation>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<visible>SubString(ListItem.Icon,Default,Left)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(58)</visible>
			<!-- Label -->
			<control type="label">
				<posx>20</posx>
				<posy>632</posy>
				<width>1240</width>
				<height>37</height>
				<font>font22</font>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<label>$INFO[ListItem.Label]</label>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>240</posx>
				<posy>632</posy>
				<width>800</width>
				<height>37</height>
				<font>font22</font>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<label>$LOCALIZE[31688]</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Resolution -->
			<control type="label">
				<posx>240</posx>
				<posy>682</posy>
				<width>800</width>
				<height>37</height>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<label>$INFO[ListItem.PictureResolution]</label>
				<textoffsetx>0</textoffsetx>
			</control>
			<control type="scrollbar" id="1358">
				<posx>1244</posx>
				<posy>102</posy>
				<width>18</width>
				<height>527</height>
				<onleft>58</onleft>
				<onright>9000</onright>
				<animation condition="!Control.HasFocus(1358) + !Container.Scrolling" effect="fade" start="100" end="0" time="200" reversible="true">Conditional</animation>
				<visible>Skin.HasSetting(sbar)</visible>
			</control>
		</control>
	</include>
</includes>
