<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog">
    <!-- Extended infos Videos -->
    <defaultcontrol always="true">300</defaultcontrol>
    <onload>ClearProperty(DialogVisibility,Home)</onload>
    <onload>ClearProperty(DialogVisibility2,Home)</onload>
    <onload>ClearProperty(DialogVisibility3,Home)</onload>
    <onload>ClearProperty(DialogVisibility4,Home)</onload>
    <onload>ClearProperty(DialogVisibility5,Home)</onload>
    
    <controls>
        
        <control type="group">
            <visible>IsEmpty(Window(Home).Property(DialogVisibility)) + IsEmpty(Window(Home).Property(DialogVisibility2)) + IsEmpty(Window(Home).Property(DialogVisibility3)) + IsEmpty(Window(Home).Property(DialogVisibility4)) + IsEmpty(Window(Home).Property(DialogVisibility5))</visible>
            
            <include name="DialogPOS">
                <param name="DialogName" value="DiaExtended2" />
            </include>              

        </control>

    </controls>

</window>