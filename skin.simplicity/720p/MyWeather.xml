				<randomize>true</randomize>
				<fadetime>1000</fadetime>
				<visible>Skin.HasSetting(ShowWeatherFanart) + !IsEmpty(Skin.String(WeatherFanartDir))</visible>
				<animation type="WindowOpen" reversible="false">
