<includes>
	<include name="osd_PlayerButton">
		<font>Symbols-26</font>
		<width>90</width>
		<align>center</align>
		<aligny>center</aligny>
		<textcolor>$VAR[color:Accent]</textcolor>
		<texturenofocus colordiffuse="$VAR[color:Main]">colorize/95.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</texturefocus>
	</include>

<!--player controls-->
	<include name="osd_MenuLayout">
		<param name="ID"/>
		<definition>
			<itemlayout width="540" height="90">
				<control type="label">
					<width>90</width>
					<height>90</height>
					<label>$INFO[ListItem.Label2]</label>
					<font>Symbols-26</font>
					<align>center</align>
					<aligny>center</aligny>
					<textcolor>$VAR[color:Accent]</textcolor>
				</control>
				<control type="label">
					<left>114</left>
					<width>540</width>
					<height>90</height>
					<label>$INFO[ListItem.Label]</label>
					<font>Regular-20</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>$VAR[color:Accent]</textcolor>
				</control>
				<control type="label">
					<right>32</right>
					<width max="270">auto</width>
					<height>90</height>
					<label>$INFO[ListItem.Property(ButtonHint)]</label>
					<font>Regular-20</font>
					<align>right</align>
					<aligny>center</aligny>
					<textcolor>$VAR[color:Text.2]</textcolor>
				</control>
			</itemlayout>
			<focusedlayout width="540" height="90">
				<control type="group">
					<visible>!Control.HasFocus($PARAM[ID])</visible>
					<control type="label">
						<width>90</width>
						<height>90</height>
						<label>$INFO[ListItem.Label2]</label>
						<font>Symbols-26</font>
						<align>center</align>
						<aligny>center</aligny>
						<textcolor>$VAR[color:Accent]</textcolor>
					</control>
					<control type="label">
						<left>114</left>
						<width>540</width>
						<height>90</height>
						<label>$INFO[ListItem.Label]</label>
						<font>Regular-20</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>$VAR[color:Accent]</textcolor>
					</control>
					<control type="label">
						<right>32</right>
						<width max="270">auto</width>
						<height>90</height>
						<label>$INFO[ListItem.Property(ButtonHint)]</label>
						<font>Regular-20</font>
						<align>right</align>
						<aligny>center</aligny>
						<textcolor>$VAR[color:Text.2]</textcolor>
					</control>
				</control>
				<control type="group">
					<visible>Control.HasFocus($PARAM[ID])</visible>
					<control type="image">
						<aspectratio>stretch</aspectratio>
						<width>540</width>
						<height>90</height>
						<texture colordiffuse="$VAR[color:Accent]">colorize/75.png</texture>
					</control>
					<control type="label">
						<width>90</width>
						<height>90</height>
						<label>$INFO[ListItem.Label2]</label>
						<font>Symbols-26</font>
						<align>center</align>
						<aligny>center</aligny>
						<textcolor>$VAR[color:Main]</textcolor>
					</control>
					<control type="label">
						<left>114</left>
						<width>540</width>
						<height>90</height>
						<label>$INFO[ListItem.Label]</label>
						<font>Regular-20</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>$VAR[color:Main]</textcolor>
					</control>
					<control type="label">
						<right>32</right>
						<width max="270">auto</width>
						<height>90</height>
						<label>$INFO[ListItem.Property(ButtonHint)]</label>
						<font>Regular-20</font>
						<align>right</align>
						<aligny>center</aligny>
						<textcolor>$VAR[color:Main]</textcolor>
					</control>
				</control>
			</focusedlayout>
		</definition>
	</include>

	<include name="osd_PlayControls">
		<param name="Visible"/>
		<definition>
			<control type="group">
				<width>540</width>
				<height>630</height>
				<include>fadeinout</include>
				<visible>$PARAM[Visible]</visible>

				<control type="image">
					<width>540</width>
					<height>90</height>
					<texture colordiffuse="ff000000">colorize/100.png</texture>
					<aspectratio>stretch</aspectratio>
				</control>
				
				<control type="grouplist" id="700">
					<onup>630</onup>
					<ondown>601</ondown>
					<itemgap>0</itemgap>
					<onleft>700</onleft>
					<onright>700</onright>
					<onup>SetFocus(800,5)</onup>
					<ondown>SetFocus(800,0)</ondown>
					<orientation>horizontal</orientation>
					<!--channel down-->
					<control type="button" id="701">
						<label>&#58955;</label>
						<include>osd_PlayerButton</include>
						<onclick>ChannelDown</onclick>
						<visible>VideoPlayer.Content(LiveTV)</visible>
					</control>
					<!--channel up-->
					<control type="button" id="702">
						<label>&#58952;</label>
						<include>osd_PlayerButton</include>
						<onclick>ChannelUp</onclick>
						<visible>VideoPlayer.Content(LiveTV)</visible>
					</control>
					<!--previous-->
					<control type="button" id="703">
						<label>&#59171;</label>
						<include>osd_PlayerButton</include>
						<onclick>PlayerControl(Previous)</onclick>
						<visible>!VideoPlayer.Content(LiveTV)</visible>
					</control>
					<!--rewind-->
					<control type="button" id="704">
						<label>&#59056;</label>
						<include>osd_PlayerButton</include>
						<onclick>PlayerControl(Rewind)</onclick>
						<visible>!VideoPlayer.Content(LiveTV)</visible>
						<enable>Player.SeekEnabled</enable>
					</control>
					<!--pause-->
					<control type="togglebutton" id="705">
						<label>&#59054;</label>
						<altlabel>&#59053;</altlabel>
						<usealttexture>Player.Paused | Player.Forwarding | Player.Rewinding</usealttexture>
						<include>osd_PlayerButton</include>
						<onclick>PlayerControl(Play)</onclick>
					</control>
					<!--stop-->
					<control type="button" id="706">
						<label>&#59051;</label>
						<include>osd_PlayerButton</include>
						<onclick>PlayerControl(Stop)</onclick>
					</control>
					<!--forward-->
					<control type="button" id="707">
						<label>&#59055;</label>
						<include>osd_PlayerButton</include>
						<onclick>PlayerControl(Forward)</onclick>
						<visible>!VideoPlayer.Content(LiveTV)</visible>
						<enable>Player.SeekEnabled</enable>
					</control>
					<!--next-->
					<control type="button" id="708">
						<label>&#59170;</label>
						<include>osd_PlayerButton</include>
						<onclick>PlayerControl(Next)</onclick>
						<visible>!VideoPlayer.Content(LiveTV)</visible>
					</control>
					<!--record-->
					<control type="button" id="709">
						<label>&#59172;</label>
						<textcolor>Attention</textcolor>
						<focusedcolor>Attention</focusedcolor>
						<onclick>PlayerControl(Record)</onclick>
						<include>osd_PlayerButton</include>
						<visible>VideoPlayer.Content(LiveTV) + Player.Recording</visible>
					</control>
					<control type="button" id="710">
						<label>&#59172;</label>
						<onclick>PlayerControl(Record)</onclick>
						<include>osd_PlayerButton</include>
						<visible>VideoPlayer.Content(LiveTV) + !Player.Recording</visible>
					</control>
					<!-- Teletext -->
					<control type="button" id="712">
						<label>&#58986;</label>
						<onclick>Dialog.Close(VideoOSD,true)</onclick>
						<include>osd_PlayerButton</include>
						<onclick>ActivateWindow(teletext)</onclick>
						<visible>VideoPlayer.Content(LiveTV)</visible>
					</control>
				</control>


				<control type="group">
					<top>90</top>
					<control type="image">
						<width>540</width>
						<height>540</height>
						<texture colordiffuse="$VAR[color:Main]">colorize/100.png</texture>
					</control>
					<control type="image">
						<width>540</width>
						<height>540</height>
						<texture colordiffuse="ff000000" flipy="true">grid/osd/90.png</texture>
						<aspectratio>scale</aspectratio>
					</control>
				</control>

				<control type="list" id="800">
					<top>90</top>
					<width>540</width>
					<height>630</height>
					<onback>Skin.Reset(OSD)</onback>
					<onleft>Skin.Reset(OSD)</onleft>
					<onup>700</onup>
					<ondown>700</ondown>
					<orientation>vertical</orientation>
					<include content="osd_MenuLayout">
						<param name="ID">800</param>
					</include>
					<content>
						<!-- Playlist -->
						<item id="1">
							<label>$LOCALIZE[13350]</label>
							<label2>&#57392;</label2>
							<onclick>Close</onclick>
							<onclick>ActivateWindow(musicplaylist)</onclick>
							<visible>Player.HasAudio</visible>
							<visible>Integer.IsGreater(Playlist.Length(music),1)</visible>
							<visible>String.IsEmpty(Skin.String(OSD))</visible>
						</item>
						<!-- Visualisation -->
						<item id="1">
							<label>$LOCALIZE[250]</label>
							<label2>&#57463;</label2>
							<onclick>Skin.SetString(OSD,Viz)</onclick>
							<onclick>Control.SetFocus(800,1)</onclick>
							<visible>Player.HasAudio</visible>
							<visible allowhiddenfocus="true">String.IsEmpty(Skin.String(OSD,Viz))</visible>
						</item>
						<item id="2">
							<label>..</label>
							<label2>&#57424;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,Viz)</visible>
						</item>
						<!-- Visualization select -->
						<item id="3">
							<label>$LOCALIZE[424]</label>
							<label2>&#57447;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>Dialog.Close(MusicOSD,true)</onclick>
							<onclick>Addon.Default.Set(xbmc.player.musicviz)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,Viz)</visible>
						</item>
						<!-- Visualization presets -->	
						<item id="4">
							<label>$INFO[Visualisation.Name] $LOCALIZE[13388]</label>
							<label2>&#58957;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>Dialog.Close(MusicOSD,true)</onclick>
							<onclick>ActivateWindow(visualisationpresetlist)</onclick>
							<visible>Player.HasAudio</visible>
							<visible allowhiddenfocus="true">Skin.String(OSD,Viz)</visible>
						</item>
						<!-- Visualization settings -->	
						<item id="5">
							<label>$INFO[Visualisation.Name] $LOCALIZE[5]</label>
							<label2>&#59014;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>Dialog.Close(MusicOSD,true)</onclick>
							<onclick>Addon.Default.OpenSettings(xbmc.player.musicviz)</onclick>
							<visible>Player.HasAudio</visible>
							<visible allowhiddenfocus="true">Skin.String(OSD,Viz)</visible>
						</item>
						<!-- Lyrics -->	
						<item id="6">
							<label>$LOCALIZE[24013]</label>
							<label2>&#57388;</label2>
							<onclick condition="System.HasAddon(script.cu.lrclyrics)">Skin.SetString(OSD,Lyrics)</onclick>
							<onclick condition="System.HasAddon(script.cu.lrclyrics)">Control.SetFocus(800,1)</onclick>
							<onclick condition="!System.HasAddon(script.cu.lrclyrics)">Addon.Default.Set(xbmc.python.lyrics)</onclick>
							<visible>Player.HasAudio</visible>
							<visible allowhiddenfocus="true">String.IsEmpty(Skin.String(OSD,Lyrics))</visible>
						</item>
						<item id="7">
							<label>..</label>
							<label2>&#57424;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,Lyrics)</visible>
						</item>
						<!-- Get Lyrics -->
						<item id="8">
							<label>$LOCALIZE[31559]</label>
							<label2>&#57475;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>Dialog.Close(MusicOSD,true)</onclick>
							<onclick>RunScript(script.cu.lrclyrics)</onclick>
							<visible>Player.HasAudio</visible>
							<visible allowhiddenfocus="true">Skin.String(OSD,Lyrics)</visible>
						</item>
						<!-- Lyrics settings -->
						<item id="9">
							<label>$LOCALIZE[24013] $LOCALIZE[5]</label>
							<label2>&#59014;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>Dialog.Close(MusicOSD,true)</onclick>
							<onclick>Addon.OpenSettings(script.cu.lrclyrics)</onclick>
							<visible>Player.HasAudio</visible>
							<visible allowhiddenfocus="true">Skin.String(OSD,Lyrics)</visible>
						</item>
						<!--random-->	
						<item id="20">
							<label>$LOCALIZE[13319]$INFO[Playlist.Random,: ]</label>
							<label2>&#57433;</label2>
							<onclick>PlayerControl(Random)</onclick>
							<visible>Player.HasAudio</visible>
							<visible>String.IsEmpty(Skin.String(OSD))</visible>
						</item>
						<!--repeat-->	
						<item id="21">
							<label>$LOCALIZE[486]$INFO[Playlist.Repeat,: ]</label>
							<label2>&#57444;</label2>
							<onclick>PlayerControl(Repeat)</onclick>
							<visible>Player.HasAudio</visible>
							<visible>String.IsEmpty(Skin.String(OSD))</visible>
						</item>
						<!-- Guide -->
						<item id="22">
							<label>$LOCALIZE[22020]$INFO[VideoPlayer.ChannelName, - ]</label>
							<label2>&#57361;</label2>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<onclick>ActivateWindow(PVROSDGuide)</onclick>
							<visible>VideoPlayer.Content(LiveTV)</visible>
						</item>
						<!-- Channels -->
						<item id="22">
							<label>$LOCALIZE[14301]</label>
							<label2>&#59138;</label2>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<onclick>ActivateWindow(PVROSDChannels)</onclick>
							<visible>VideoPlayer.Content(LiveTV)</visible>
						</item>
						<!-- DVD Menu -->
						<item id="23">
							<label>$LOCALIZE[33061]</label>
							<label2>&#59138;</label2>
							<onclick>PlayerControl(ShowVideoMenu)</onclick>
							<visible>VideoPlayer.HasMenu</visible>
							<visible>String.IsEmpty(Skin.String(OSD))</visible>
						</item>
						<!--subtitles-->
						<item id="30">
							<label>$LOCALIZE[287]</label>
							<label2>&#58947;</label2>
							<onclick>Skin.SetString(OSD,Subtitles)</onclick>
							<onclick>Control.SetFocus(800,1)</onclick>
							<visible>Player.HasVideo</visible>
							<visible>!VideoPlayer.Content(LiveTV)</visible>
							<visible allowhiddenfocus="true">String.IsEmpty(Skin.String(OSD,Subtitles))</visible>
						</item>
						<!--back-->
						<item id="31">
							<label>..</label>
							<label2>&#57424;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,Subtitles)</visible>
						</item>
						<!--get subtitles-->
						<item id="32">
							<label>$LOCALIZE[31557]</label>
							<label2>&#57475;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<onclick>ActivateWindow(SubtitleSearch)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,Subtitles)</visible>
						</item>
						<!--subtitle offset-->
						<item id="33">
							<label>$LOCALIZE[22006]</label>
							<label2>&#58968;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>SubtitleDelay</onclick>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,Subtitles)</visible>
						</item>
						<!--toggle subtitles-->
						<item id="34">
							<label>$VAR[player:Video.SubtitlesLanguage]</label>
							<label2>$VAR[player:Video.SubtitlesEnabled]</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>ShowSubtitles</onclick>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,Subtitles)</visible>
						</item>
						<!--3D-->
						<item id="40">
							<label>3D</label>
							<label2>&#57373;</label2>
							<onclick>Skin.SetString(OSD,3D)</onclick>
							<visible>Player.HasVideo</visible>
							<visible allowhiddenfocus="true">String.IsEmpty(Skin.String(OSD,3D))</visible>
							<enable>VideoPlayer.IsStereoscopic</enable>
						</item>
						<item id="41">
							<label>..</label>
							<label2>&#57424;</label2>
							<onclick>Skin.Reset(OSD)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,3D)</visible>
						</item>
						<item id="42">
							<label>$VAR[player:3D.Label]</label>
							<label2>$VAR[player:3D.Symbol]</label2>
							<onclick>StereoModeToMono</onclick>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,3D)</visible>
						</item>
						<item id="43">
							<label>$LOCALIZE[31562]</label>
							<label2>&#57479;</label2>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<onclick>StereoMode</onclick>
							<visible allowhiddenfocus="true">Skin.String(OSD,3D)</visible>
						</item>
						<!--bookmarks-->
						<item>
							<label>$LOCALIZE[298]</label>
							<label2>&#59207;</label2>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>ActivateWindow(videobookmarks)</onclick>
							<visible>Player.HasVideo</visible>
							<visible>!VideoPlayer.Content(LiveTV)</visible>
							<visible>String.IsEmpty(Skin.String(OSD))</visible>
						</item>
						<!--video settngs-->
						<item>
							<label>$LOCALIZE[13395]</label>
							<label2>&#57352;</label2>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>ActivateWindow(osdvideosettings)</onclick>
							<visible>Player.HasVideo</visible>
							<visible>String.IsEmpty(Skin.String(OSD))</visible>
						</item>
						<!--audio settings-->
						<item>
							<label>$LOCALIZE[13396]</label>
							<label2>&#57504;</label2>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>ActivateWindow(osdaudiosettings)</onclick>
							<visible>Player.HasVideo</visible>
							<visible>String.IsEmpty(Skin.String(OSD))</visible>
						</item>
						<item>
							<label>$VAR[player:InfoToggle]</label>
							<label2>&#57483;</label2>
							<onclick>Dialog.Close(VideoOSD,true)</onclick>
							<onclick>Skin.Reset(OSD)</onclick>
							<onclick>Action(Info)</onclick>
							<visible>String.IsEmpty(Skin.String(OSD))</visible>
						</item>
					</content>
				</control>
			</control>
		</definition>
	</include>

	<!-- Fullscreen info -->
	<include name="osd_Info">
		<param name="Art" default="$INFO[MusicPlayer.Cover]"/>
		<param name="ArtPosY" default="64"/>
		<param name="ArtSize" default="200"/>
		<param name="Description" default="$VAR[player:Info]"/>
		<param name="DescriptionLeft" default="0"/>
		<param name="DescriptionWidth" default="1664"/>
		<param name="Label" default="$VAR[player:Title]"/>
		<param name="Label2" default="$VAR[player:Info]"/>
		<param name="ShowArt" default="false"/>
		<param name="TextLeft" default="0"/>
		<param name="TextWidth" default="1664"/>
		<param name="Top" default="750"/>
		<param name="Visible">[Player.Paused + !Player.HasGame] | Player.ShowInfo | Window.IsActive(DialogFullScreenInfo.xml)</param>
		<definition>
			<control type="image">
				<width>screen_Width</width>
				<height>screen_Height</height>
				<include>transition_Content</include>
				<texture colordiffuse="ff000000">views/diffuse/fullscreeninfo.png</texture>
				<animation effect="fade" end="60" time="0" condition="Window.IsActive(visualisation)">Conditional</animation>
				<visible>![Player.Paused + Skin.HasSetting(osd.pauseinfo)]</visible>
				<visible>$PARAM[Visible]</visible>
			</control>
			<control type="group">
				<left>128</left>
				<top>$PARAM[Top]</top>
				<height>285</height>
				<visible>![Player.Paused + Skin.HasSetting(osd.pauseinfo)]</visible>
				<visible>$PARAM[Visible]</visible>
				<include>transition_Content</include>
				<control type="image">
					<top>$PARAM[ArtPosY]</top>
					<width>$PARAM[ArtSize]</width>
					<height>$PARAM[ArtSize]</height>
					<texture>$PARAM[Art]</texture>
					<aspectratio align="center">keep</aspectratio>
					<visible>$PARAM[ShowArt]</visible>
				</control>
				<control type="label">
					<bottom>170</bottom>
					<left>$PARAM[TextLeft]</left>
					<label>$PARAM[Label]</label>
					<width>$PARAM[TextWidth]</width>
					<height>28</height>
					<font>Black-54</font>
					<aligny>center</aligny>
					<textcolor>$VAR[color:Accent]</textcolor>
				</control>
				<control type="label">
					<left>$PARAM[TextLeft]</left>
					<bottom>130</bottom>
					<height>24</height>
					<width>$PARAM[TextWidth]</width>
					<label>$PARAM[Label2]</label>
					<font>Black-24</font>
					<aligny>center</aligny>
					<textcolor>$VAR[color:Text.2]</textcolor>
				</control>
				<control type="textbox">
					<left>$PARAM[DescriptionLeft]</left>
					<bottom>0</bottom>
					<width>$PARAM[TextWidth]</width>
					<height>115</height>
					<label>$VAR[player:Description]</label>
					<font>Slab-24</font>
					<textcolor>$VAR[color:Accent]</textcolor>
				</control>
			</control>
		</definition>
	</include>

	<!-- Game OSD -->
	<include name="osd_Game">
		<control type="group">
			<include>fadeinout</include>
			<control type="group">
				<control type="image">
					<width>540</width>
					<height>360</height>
					<texture colordiffuse="$VAR[color:Main]">colorize/100.png</texture>
				</control>
				<control type="image">
					<width>540</width>
					<height>360</height>
					<texture colordiffuse="ff000000" flipy="true" border="0,0,0,360">grid/osd/90.png</texture>
					<aspectratio>scale</aspectratio>
				</control>
			</control>
			<control type="list" id="800">
				<defaultcontrol always="true">801</defaultcontrol>
				<width>540</width>
				<height>800</height>
				<onback>Skin.Reset(OSD)</onback>
				<onleft>Skin.Reset(OSD)</onleft>
				<onup>800</onup>
				<ondown>800</ondown>
				<orientation>vertical</orientation>
				<include content="osd_MenuLayout">
					<param name="ID">800</param>
				</include>
				<content>
					<!-- Pause / resume -->
					<item id="801">
						<label>$LOCALIZE[35224]</label>
						<label2>&#59053;</label2>
						<property name="ButtonHint">Select + X</property>
						<onclick>Play</onclick>
						<visible>String.IsEmpty(Skin.String(OSD))</visible>
					</item>
					<!-- Reset -->
					<item id="802">
						<label>$LOCALIZE[10035]</label>
						<label2>&#57497;</label2>
						<property name="ButtonHint">Select + B</property>
						<onclick>PlayerControl(Reset)</onclick>
						<visible>String.IsEmpty(Skin.String(OSD))</visible>
					</item>
					<!-- Stop -->
					<item id="803">
						<label>$LOCALIZE[35222]</label>
						<label2>&#57445;</label2>
						<property name="ButtonHint">Select + Start</property>
						<onclick>Stop</onclick>
						<visible>String.IsEmpty(Skin.String(OSD))</visible>
					</item>
					<!-- Settings -->
					<item id="810">
						<label>$LOCALIZE[5]</label>
						<label2>&#59014;</label2>
						<onclick>Skin.SetString(OSD,Game)</onclick>
						<onclick>Control.SetFocus(800,1)</onclick>
						<visible>String.IsEmpty(Skin.String(OSD))</visible>
					</item>
					<!-- Up -->
					<item id="811">
						<label>..</label>
						<label2>&#57424;</label2>
						<onclick>Skin.Reset(OSD)</onclick>
						<visible allowhiddenfocus="true">Skin.String(OSD,Game)</visible>
					</item>
					<!-- Video Filter -->
					<item id="812">
						<label>$LOCALIZE[35225]</label>
						<label2>&#59082;</label2>
						<onclick>Skin.Reset(OSD)</onclick>
						<onclick>Close</onclick>
						<onclick>ActivateWindow(GameVideoFilter)</onclick>
						<visible allowhiddenfocus="true">Skin.String(OSD,Game)</visible>
					</item>
					<!-- View mode -->
					<item id="813">
						<label>$LOCALIZE[629]</label>
						<label2>&#59119;</label2>
						<onclick>Skin.Reset(OSD)</onclick>
						<onclick>Close</onclick>
						<onclick>ActivateWindow(GameViewMode)</onclick>
						<visible allowhiddenfocus="true">Skin.String(OSD,Game)</visible>
					</item>
					<!-- Input settings -->
					<item id="814">
						<label>$LOCALIZE[35223]</label>
						<label2>&#57371;</label2>
						<onclick>Skin.Reset(OSD)</onclick>
						<onclick>Close</onclick>
						<onclick>ActivateWindow(GameControllers)</onclick>
						<visible allowhiddenfocus="true">Skin.String(OSD,Game)</visible>
					</item>
				</content>
			</control>
		</control>
	</include>
</includes>