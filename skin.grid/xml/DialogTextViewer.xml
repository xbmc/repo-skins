<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">6500</defaultcontrol>
	<controls>

		<!--global includes-->
		<include>background_Global</include>

		<!--window chrome-->		
		<control type="group">
			<include>transition_Chrome</include>
			<include content="global_WindowChrome">
				<param name="Title">$VAR[window:Title.Dialog]</param>
			</include>
		</control>

		<control type="textbox" id="5">
			<left>160</left>
			<top>222</top>
			<width>1632</width>
			<height>794</height>
			<textcolor>$VAR[color:Accent]</textcolor>
			<font>Slab-28</font>
			<pagecontrol>6500</pagecontrol>
			<include>transition_Content</include>
		</control>
		
		<!--scrollbars-->
		<include>global_Scrollbar</include>

	</controls>
</window>