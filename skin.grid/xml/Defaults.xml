<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<default type="multiimage">
		<aspectratio>scale</aspectratio>
		<fadetime>fadetime_Slideshow</fadetime>
		<include condition="!Window.IsActive(Home)">Burns</include>
		<include>screen_Dimensions</include>
		<pauseatend>0</pauseatend>
		<randomize>true</randomize>
		<include>Burns</include>
		<timeperimage>30000</timeperimage>
	</default>

	<default type="label">
		<font>Slab-32</font>
		<height>90</height>
		<aligny>center</aligny>
		<textcolor>$VAR[color:Text.3]</textcolor>
		<disabledcolor>$VAR[color:Disabled]</disabledcolor>
		<selectedcolor>$VAR[color:Main]</selectedcolor>
		<scroll>false</scroll>
		<scrollspeed>autoscroll_Line</scrollspeed>
		<scrollsuffix> | </scrollsuffix>
	</default>

	<default type="edit">
		<width min="128">auto</width>
		<height>90</height>
		<font>Regular-22-L</font>
		<align>left</align>
		<aligny>center</aligny>
		<textoffsetx>32</textoffsetx>
		<hinttext>-</hinttext>
		<textcolor>$VAR[color:Accent]</textcolor>
		<focusedcolor>$VAR[color:Main]</focusedcolor>
		<disabledcolor>$VAR[color:Disabled]</disabledcolor>
		<invalidcolor>Warning</invalidcolor>
		<pulseonselect>false</pulseonselect>
		<texturenofocus colordiffuse="ff000000">colorize/10.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</texturefocus>
	</default>
	
	<default type="button">
		<width>108</width>
		<height>90</height>
		<aligny>center</aligny>
		<font>Regular-22-L</font>
		<textoffsetx>32</textoffsetx>
		<textcolor>$VAR[color:Accent]</textcolor>
		<focusedcolor>$VAR[color:Main]</focusedcolor>
		<disabledcolor>$VAR[color:Disabled]</disabledcolor>
		<selectedcolor>$VAR[color:Accent]</selectedcolor>
		<texturenofocus colordiffuse="ff000000">colorize/10.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="togglebutton">
		<width>128</width>
		<height>90</height>
		<aligny>center</aligny>
		<font>Regular-22-L</font>
		<textoffsetx>32</textoffsetx>
		<textcolor>$VAR[color:Accent]</textcolor>
		<focusedcolor>$VAR[color:Main]</focusedcolor>
		<disabledcolor>$VAR[color:Disabled]</disabledcolor>
		<selectedcolor>$VAR[color:Accent]</selectedcolor>
		<texturenofocus colordiffuse="ff000000">colorize/10.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</texturefocus>
		<alttexturenofocus colordiffuse="ff000000">colorize/10.png</alttexturenofocus>
		<alttexturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</alttexturefocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="radiobutton">
		<width>128</width>
		<height>90</height>
		<aligny>center</aligny>
		<font>Regular-22-L</font>
		<textoffsetx>32</textoffsetx>
		<textcolor>$VAR[color:Accent]</textcolor>
		<focusedcolor>$VAR[color:Main]</focusedcolor>
		<disabledcolor>$VAR[color:Disabled]</disabledcolor>
		<selectedcolor>$VAR[color:Accent]</selectedcolor>
		<texturenofocus colordiffuse="ff000000">colorize/10.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</texturefocus>
		<textureradioonnofocus colordiffuse="$VAR[color:Accent]">glyphs/radio/on.png</textureradioonnofocus>
		<textureradiooffnofocus colordiffuse="$VAR[color:Accent]">glyphs/radio/off.png</textureradiooffnofocus>
		<textureradioonfocus colordiffuse="$VAR[color:Main]">glyphs/radio/on.png</textureradioonfocus>
		<textureradioofffocus colordiffuse="$VAR[color:Main]">glyphs/radio/off.png</textureradioofffocus>
		<textureradioondisabled colordiffuse="Disabled">glyphs/radio/on.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="Disabled">glyphs/radio/off.png</textureradiooffdisabled>
		<radiowidth>90</radiowidth>		
		<radioheight>90</radioheight>
		<pulseonselect>false</pulseonselect>
		<radioposx></radioposx>
		<radioposy></radioposy>
	</default>

	<default type="spincontrolex">
		<height>90</height>
		<font>Regular-22-L</font>
		<aligny>center</aligny>
		<textcolor>$VAR[color:Accent]</textcolor>
		<focusedcolor>$VAR[color:Main]</focusedcolor>
		<disabledcolor>$VAR[color:Disabled]</disabledcolor>
		<textoffsetx>32</textoffsetx>
		<texturenofocus colordiffuse="ff000000">colorize/10.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</texturefocus>
		<textureup colordiffuse="$VAR[color:Accent]">glyphs/spin/up-nf.png</textureup>
		<textureupfocus colordiffuse="$VAR[color:Main]">glyphs/spin/up-fo.png</textureupfocus>
		<texturedown colordiffuse="$VAR[color:Accent]">glyphs/spin/down-nf.png</texturedown>
		<texturedownfocus colordiffuse="$VAR[color:Main]">glyphs/spin/down-fo.png</texturedownfocus>
		<textureupdisabled colordiffuse="$VAR[color:Disabled]">glyphs/spin/up-nf.png</textureupdisabled>
		<texturedowndisabled colordiffuse="$VAR[color:Disabled]">glyphs/spin/down-nf.png</texturedowndisabled>	
		<spinwidth>90</spinwidth>
		<spinheight>90</spinheight>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrol">
		<height>90</height>
		<font>Regular-22-L</font>
		<aligny>center</aligny>
		<textcolor>$VAR[color:Accent]</textcolor>
		<focusedcolor>$VAR[color:Main]</focusedcolor>
		<disabledcolor>$VAR[color:Disabled]</disabledcolor>
		<textoffsetx>32</textoffsetx>
		<texturenofocus colordiffuse="ff000000">colorize/10.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</texturefocus>
		<textureup colordiffuse="$VAR[color:Accent]">glyphs/spin/up-nf.png</textureup>
		<textureupfocus colordiffuse="$VAR[color:Main]">glyphs/spin/up-fo.png</textureupfocus>
		<texturedown colordiffuse="$VAR[color:Accent]">glyphs/spin/down-nf.png</texturedown>
		<texturedownfocus colordiffuse="$VAR[color:Main]">glyphs/spin/down-fo.png</texturedownfocus>
		<textureupdisabled colordiffuse="$VAR[color:Disabled]">glyphs/spin/up-nf.png</textureupdisabled>
		<texturedowndisabled colordiffuse="$VAR[color:Disabled]">glyphs/spin/down-nf.png</texturedowndisabled>	
		<spinwidth>90</spinwidth>
		<spinheight>90</spinheight>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="sliderex">
		<height>90</height>
		<font>Regular-22-L</font>
		<aligny>center</aligny>
		<textoffsetx>32</textoffsetx>
		<textcolor>$VAR[color:Accent]</textcolor>
		<focusedcolor>$VAR[color:Main]</focusedcolor>
		<disabledcolor>$VAR[color:Disabled]</disabledcolor>
		<selectedcolor>$VAR[color:Accent]</selectedcolor>
		<texturenofocus colordiffuse="ff000000">colorize/10.png</texturenofocus>
		<texturefocus colordiffuse="$VAR[color:Accent]">colorize/75.png</texturefocus>
		<sliderwidth>256</sliderwidth>
		<sliderheight>54</sliderheight>
		<texturesliderbar colordiffuse="$VAR[color:Accent]">glyphs/slider/bar_small.png</texturesliderbar>
		<texturesliderbarfocus colordiffuse="$VAR[color:Main]">glyphs/slider/bar_small.png</texturesliderbarfocus>
		<textureslidernib colordiffuse="$VAR[color:Accent]">glyphs/slider/nib_small.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[color:Main]">glyphs/slider/nib_small.png</textureslidernibfocus>
		<pulseonselect>false</pulseonselect>
		<orientation>horizontal</orientation>
	</default>

	<default type="slider">
		<width>300</width>
		<height>20</height>
		<orientation>horizontal</orientation>
		<texturesliderbar colordiffuse="$VAR[color:Main]">glyphs/slider/bar.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[color:Main]">glyphs/slider/nib-nf.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[color:Accent]">glyphs/slider/nib-fo.png</textureslidernibfocus>
		<controloffsetx>10</controloffsetx>
	</default>

	<default type="textbox">
		<align>left</align>
		<font>Slab-22</font>
		<height>auto</height>ยง
		<aligny>top</aligny>
		<textcolor>$VAR[color:Accent]</textcolor>
		<include condition="String.IsEqual(Skin.String(autoscroll.speed),none)">autoscroll_None</include>
		<include condition="String.IsEqual(Skin.String(autoscroll.speed),normal)">autoscroll_Normal</include>
		<include condition="String.IsEqual(Skin.String(autoscroll.speed),slow)">autoscroll_Slow</include>
		<include condition="String.IsEqual(Skin.String(autoscroll.speed),fast)">autoscroll_Fast</include>
		<scrolltime>200</scrolltime>
	</default>
	
	<default type="fadelabel">
	</default>

	<default type="multiimage">
		<include>screen_Dimensions</include>
		<aspectratio align="center">scale</aspectratio>
		<randomize>true</randomize>
		<loop>yes</loop>
		<timeperimage>slideshow_Timer</timeperimage>
		<fadetime>fadetime_Slideshow</fadetime>
	</default>

	<default type="progress">
		<texturebg border="0" colordiffuse="$VAR[color:Text.3]">colorize/75.png</texturebg>
		<lefttexture border="0" colordiffuse="$VAR[color:Accent]">colorize/75.png</lefttexture>
		<midtexture border="0" colordiffuse="$VAR[color:Accent]">colorize/75.png</midtexture>
		<midtexture border="0" colordiffuse="$VAR[color:Accent]">colorize/75.png</midtexture>
		<righttexture border="0" colordiffuse="$VAR[color:Accent]">colorize/75.png</righttexture>
		<overlaytexture></overlaytexture>
	</default>

	<default type="grouplist">ยง
		<itemgap>0</itemgap>
	</default>

	<default type="scrollbar">
		<showonepage>false</showonepage>
		<texturesliderbackground colordiffuse="ffffffff">colorize/10.png</texturesliderbackground>		
		<texturesliderbar colordiffuse="$VAR[color:Accent]">colorize/25.png</texturesliderbar>
		<texturesliderbarfocus colordiffuse="$VAR[color:Text.2]">colorize/75.png</texturesliderbarfocus>
		<textureslidernib>-</textureslidernib>
		<textureslidernibfocus>-</textureslidernibfocus>
	</default>

</includes>