			  <bordersize>ThumbsBordersize6pix</bordersize>
		    <visible>[Window.IsVisible(Home) | Window.IsVisible(PlayerControls) | Window.IsVisible(script-globalsearch-main.xml)] + String.IsEmpty(ListItem.Art(thumb))</visible>
