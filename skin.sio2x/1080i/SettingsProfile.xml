                <posx>0</posx>
                <top>0</top>
                <width>900</width>
                <height>730</height>
                <onleft>4</onleft>
                <onright>noop</onright>
                <onup>2</onup>
                <ondown>2</ondown>
                <pagecontrol>60</pagecontrol>
                <scrolltime>280</scrolltime>
                <itemlayout height="81">
                    <control type="image">
                        <width>80</width>
                        <height>80</height>
                        <aspectratio>keep</aspectratio>
                        <info>ListItem.Icon</info>
                    </control>
                    <control type="label" description="Main Label">
                        <posx>90</posx>
                        <height>81</height>
                        <width>900</width>
                        <aligny>center</aligny>
                        <font>size36</font>
                        <textcolor>White</textcolor>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                    <include>listline</include>
                </itemlayout>
                <focusedlayout height="81">
                    <include>SmallSelector</include>
                    <control type="image">
                        <width>80</width>
                        <height>80</height>
                        <aspectratio>keep</aspectratio>
                        <info>ListItem.Icon</info>
                    </control>
                    <control type="label" description="Main Label">
                        <posx>90</posx>
                        <height>81</height>
                        <width>900</width>
                        <aligny>center</aligny>
                        <font>size36</font>
                        <textcolor>black</textcolor>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                    <include>listline</include>
                </focusedlayout>
            </control>
        </control>
        <include>User</include>
        <control type="image">
            <posx>180</posx>
            <top>200</top>
            <width>600</width>
            <height>700</height>
            <texture fallback="def_user.png">$INFO[Container(2).ListItem.Icon]</texture>
            <aspectratio>keep</aspectratio>
        </control>
        <control type="image">
            <top>100</top>
            <posx>959</posx>
            <width>1</width>
            <height>1080</height>
