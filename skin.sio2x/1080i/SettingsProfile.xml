<?xml version="1.0" encoding="UTF-8"?>
<window id="34">
    <defaultcontrol always="true">2</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <backgroundcolor>0xffffffff</backgroundcolor>
    <include>OpenClose</include>
    <controls>
        <include>BlackBackground</include>
        <include>WindowsTopInfos</include>
        <control type="togglebutton" id="4" description="Toggle Login Screen">
            <posx>80</posx>
            <top>100</top>
            <height>81</height>
            <width>800</width>
            <label>$LOCALIZE[20096]:  $LOCALIZE[1223]</label>
            <altlabel>$LOCALIZE[20096]:  $LOCALIZE[305]</altlabel>
            <align>center</align>
            <onright>2</onright>
            <onleft>2</onleft>
            <onup>2</onup>
            <ondown>2</ondown>
            <font>size36</font>
            <textoffsetx>20</textoffsetx>
        </control>

        <control type="group" description="List">
            <posx>960</posx>
            <top>100</top>
