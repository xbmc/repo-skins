			<include>animation_visible_hidden</include>
			<include>animation_hoverslide</include>
		</control>	
		<control type="image">
			<posx>0</posx>
			<posy>200</posy>
			<width>1893</width>
			<height>646</height>
			<colordiffuse>aaFFFFFF</colordiffuse>
			<texture>views/filemanager_panel.png</texture>
