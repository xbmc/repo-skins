	<!--Animation-->
	<include name="animation_hoverslide">					<!--Hover Panel Animation-->	
		<animation type="WindowOpen" reversible="false">
			 <effect type="fade" tween="sine" easing="inout" time="600"/>
		 </animation>		
		<animation type="WindowClose" reversible="false">				
           <effect type="slide" start="0" end="-1980" tween="cubic" easing="inout" time="1000"/>
           <effect type="fade" start="100" end="60" tween="sine" easing="inout" time="1000"/>
        </animation>
	</include>	
	<include name="animation_visible_hidden">				<!--Visible/Hidden Aniamtion-->	
		<animation time="600" effect="fade">Visible</animation> 
		<animation time="600" effect="fade">Hidden</animation>	
	</include>
	<include name="animation_fade">							<!--Open/Close Aniamtion-->	
		<animation effect="fade" time="600">WindowOpen</animation>		
		<animation effect="fade" time="600">WindowClose</animation>	
	</include>
	<include name="animation_sub_menu">						<!--Sub Menu Slide Animation-->	
		<animation type="Conditional" condition="!ControlGroup(9000).HasFocus()">	
			<effect type="slide" start="0" end="-600" time="450" reversible="true" tween="cubic" easing="inout"/>			
		</animation>	
	</include>
	<include name="animation_dialog">						<!--Dialog Animation-->	
		<animation type="WindowOpen" reversible="false">
			<effect type="zoom" start="75" end="100" center="960,540" easing="out" tween="back" time="300" />
			<effect type="fade" start="0" end="100" time="175" />
		</animation>
		<animation type="WindowClose" reversible="false">
			<effect type="zoom" start="100" end="175" center="960,540" easing="out" tween="sine" time="300" />
			<effect type="fade" start="100" end="0" time="250" easing="out" tween="cubic" />
		</animation>
		<animation type="Visible" reversible="false">			
			<effect type="zoom" start="75" end="100" center="960,540" easing="out" tween="back" time="300" />
			<effect type="fade" start="0" end="100" time="175" />
		</animation>
		<animation type="Hidden" reversible="false">				
			<effect type="fade" start="100" end="0" time="175" easing="out" tween="cubic" />
		</animation>
	</include>
	<include name="animation_static_dialog">				<!--Static Dialog (KaiToast,ExtendesProgress,MuteBug,Volume) Animation-->	
		<animation type="conditional" condition="Window.Next(home.xml)">		<!--Fade Home-->			
