	<!--Time & Weather-->
	<include name="TimeWeather">
		<control type="group">

			<include>animation_visible_hidden</include>
			<include>animation_fade</include>

			<control type="grouplist">	<!--Time Grouplist Line 1-->	
				<animation effect="slide" start="0" end="-130" time="200" condition="Weather.IsFetched" >Conditional</animation>
				<posx>1260</posx>
				<posy>20</posy>
				<width>600</width>
				<height>50</height>
				<align>right</align>
				<itemgap>4</itemgap>
				<orientation>horizontal</orientation>
				<usecontrolcoords>true</usecontrolcoords>			
				<control type="label">	<!--Time-->			
					<width min="0" max="200">auto</width>
					<height>50</height>
					<info>System.Time(hh:mm)</info>
					<font>Light45</font>				
					<textcolor>mainblue</textcolor>
				</control>
				<control type="label">	<!--AM-->
					<visible>substring(system.time,AM,right)</visible>
					<width min="0" max="100">auto</width>
					<height>50</height>
					<label>AM</label>
					<font>Light45</font>				
					<textcolor>mainblue</textcolor>
				</control>					
				<control type="label">	<!--PM-->
					<visible>substring(system.time,PM,right)</visible>
					<width min="0" max="100">auto</width>
					<height>50</height>
					<label>PM</label>
					<font>Light45</font>				
					<textcolor>mainblue</textcolor>
				</control>	
			</control>
			<control type="grouplist">	<!--Date Grouplist Line 2-->
				<animation effect="slide" start="0" end="-130" time="200" condition="Weather.IsFetched" >Conditional</animation>
				<posx>1260</posx>
				<posy>60</posy>
				<width>600</width>
				<height>50</height>
				<align>right</align>
				<itemgap>4</itemgap>
				<orientation>horizontal</orientation>
				<usecontrolcoords>true</usecontrolcoords>
				<include>animation_visible_hidden</include>
				<animation effect="fade" time="600" >WindowOpen</animation>		
				<animation effect="fade" time="600" >WindowClose</animation>			
				<control type="label">	<!--Day-->					
					<width min="0" max="400">auto</width>
					<height>50</height>
					<info>System.Date(DDD)</info>
					<font>Light30</font>				
					<textcolor>bbwhite</textcolor>
				</control>
				<control type="label">			
					<width min="0" max="400">auto</width>
					<height>50</height>
					<label>, </label>
					<font>Light30</font>				
					<textcolor>bbwhite</textcolor>
				</control>		
				<control type="label">	<!--Date-->						
					<width min="0" max="400">auto</width>
					<height>50</height>
					<info>System.Date(d)</info>
					<font>Light30</font>				
					<textcolor>bbwhite</textcolor>
				</control>
				<control type="label">		<!--German-->
					<visible>SubString(System.Language,German)</visible>
					<width min="0" max="400">auto</width>
					<height>50</height>
					<label>. </label>
					<font>Light30</font>				
					<textcolor>bbwhite</textcolor>
				</control>
				<control type="label">		<!--Other-->
					<visible>!SubString(System.Language,German)</visible>
					<width min="0" max="400">auto</width>
					<height>50</height>
					<label> </label>
					<font>Light30</font>				
					<textcolor>bbwhite</textcolor>
				</control>		
				<control type="label">	<!--Month-->					
					<width min="0" max="400">auto</width>
					<height>50</height>
					<info>System.Date(MMM)</info>
					<font>Light30</font>				
					<textcolor>bbwhite</textcolor>
				</control>	
				<control type="label">					
					<width min="0" max="400">auto</width>
					<height>50</height>
					<label> </label>
					<font>Light30</font>				
					<textcolor>bbwhite</textcolor>
				</control>				
				<control type="label">	<!--Weather-->					
					<width min="0" max="200">auto</width>
					<height>50</height>
					<label> | </label>
					<font>Light30</font>
					<align>left</align>			
					<textcolor>bbwhite</textcolor>
					<visible>Weather.IsFetched</visible>
				</control>		
				<control type="label">				
					<width min="0" max="200">auto</width>
					<height>50</height>
					<info>Weather.Temperature</info>
					<font>Light30</font>
					<align>left</align>				
					<textcolor>bbwhite</textcolor>
					<visible>Weather.IsFetched</visible>
				</control>			
			</control>			
			<control type="image">		<!--Weather Icon-->		
				<posy>5</posy>
				<posx>1750</posx>
				<height>110</height>
