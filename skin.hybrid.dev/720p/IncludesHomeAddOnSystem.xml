	<include name="HomeAddonsCommonLayout">
		<include>Window_OpenClose_Animation</include>
		<hitrect x="-360" y="50" w="1640" h="95" />
		<left>360</left>
		<top>40</top>
		<width>560</width>
		<height>130</height>
		<pagecontrol>-</pagecontrol>
		<scrolltime>300</scrolltime>
		<movement>2</movement>
		<orientation>Horizontal</orientation>
		<itemlayout height="110" width="112">
			<control type="image">
				<description>background</description>
				<left>5</left>
				<top>5</top>
				<width>100</width>
				<height>100</height>
				<texture border="15">RecentAddedBack.png</texture>
			</control>
			<control type="image">
				<left>15</left>
				<top>15</top>
				<width>80</width>
				<height>80</height>
				<aspectratio aligny="bottom">keep</aspectratio>
				<texture background="true" diffuse="InfoWidget/PosterDiffuse.png">$INFO[ListItem.icon]</texture>
			</control>
			<control type="image">
				<left>5</left>
				<top>5</top>
				<width>100</width>
				<height>100</height>
				<aspectratio>stretch</aspectratio>
				<texture diffuse="InfoWidget/PosterDiffuse.png">black-back.png</texture>
				<colordiffuse>AAFFFFFF</colordiffuse>
			</control>
		</itemlayout>
		<focusedlayout height="110" width="112">
			<control type="image">
				<description>background</description>
				<left>5</left>
				<top>5</top>
				<width>100</width>
				<height>100</height>
				<texture border="15">RecentAddedBack.png</texture>
				<animation effect="zoom" start="5,5,100,100" end="0,0,110,110" time="200">focus</animation>
			</control>
			<control type="image">
				<left>15</left>
				<top>15</top>
				<width>80</width>
				<height>80</height>
				<aspectratio aligny="bottom">keep</aspectratio>
				<texture background="true" diffuse="InfoWidget/PosterDiffuse.png">$INFO[ListItem.icon]</texture>
				<animation effect="zoom" start="15,15,80,80" end="10,10,90,90" time="200">focus</animation>
			</control>
			<control type="image">
				<left>5</left>
				<top>5</top>
				<width>100</width>
				<height>100</height>
				<aspectratio>stretch</aspectratio>
				<texture diffuse="InfoWidget/PosterDiffuse.png">black-back.png</texture>
				<colordiffuse>AAFFFFFF</colordiffuse>
				<animation effect="zoom" start="5,5,100,100" end="0,0,110,110" time="200">focus</animation>
				<visible>!ControlGroup(9003).HasFocus</visible>
			</control>
		</focusedlayout>
	</include>
