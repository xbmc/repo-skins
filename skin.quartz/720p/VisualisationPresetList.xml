<window id="122">
  <defaultcontrol always="true">2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <controls>
    <control type="label">
      <posy>40</posy>
      <include>DialogID</include>
      <label>VisualisationPresetList.xml</label>
    </control>
    <control type="image">
      <animation effect="zoom" start="90" end="100" time="200" center="640,360">WindowOpen</animation>
      <posx>140</posx>
      <posy>-50</posy>
      <width>1000</width>
      <height>740</height>
      <texture border="150">img/Dialog.png</texture>
    </control>
    <control type="group">
      <animation effect="zoom" start="90" end="100" time="200" center="640,360">WindowOpen</animation>
      <posx>340</posx>
      <posy>100</posy>
      <control type="label" description="Header Label">
        <posx>10</posx>
        <posy>0</posy>
        <width>560</width>
        <label>31048</label>
        <font>Title</font>
        <align>center</align>
      </control>
      <control type="fixedlist" id="2">
        <description>Preset List</description>
        <posx>10</posx>
        <posy>80</posy>
        <width>580</width>
        <height>322</height>
        <onleft>-</onleft>
        <onright>60</onright>
        <pagecontrol>60</pagecontrol>
        <focusposition>3</focusposition>
        <movement>3</movement>
        <onup>-</onup>
        <ondown>-</ondown>
        <itemlayout height="46">
          <control type="label">
            <posx>20</posx>
            <width>540</width>
            <height>46</height>
            <font>Small</font>
            <info>ListItem.Label</info>
            <scroll>false</scroll>
          </control>
        </itemlayout>
        <focusedlayout height="46">
          <control type="image">
            <visible>!Control.HasFocus(60)</visible>
            <width>580</width>
            <height>46</height>
            <texture border="8">img/ButtonFO.png</texture>
          </control>
          <control type="label">
            <posx>20</posx>
            <height>46</height>
            <width>540</width>
            <font>Small</font>
            <info>ListItem.Label</info>
          </control>
        </focusedlayout>
      </control>
    </control>
    <control type="scrollbar" id="60">
      <visible>![Skin.HasSetting(KioskEnabled) + Skin.HasSetting(HideScrollbar)]</visible>
      <animation condition="!Control.HasFocus(60) + !Container.Scrolling" effect="fade" start="100" end="70" time="200" reversible="true">Conditional</animation>
      <animation effect="zoom" start="90" end="100" time="200" center="640,360">WindowOpen</animation>
      <posx>970</posx>
      <posy>180</posy>
      <height>320</height>
      <width>20</width>
      <onleft>2</onleft>
      <onright>2</onright>
    </control>
    <include>Debug_Grid</include>
  </controls>
</window>
