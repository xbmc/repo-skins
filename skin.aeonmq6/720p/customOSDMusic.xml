	</control>
	<control type="multiimage" id="5050"><!-- Artist SlideShow Mosaico -->
	<include>MosaicBG</include>
	<left>426.5</left>
	<top>360</top>
	<visible>StringCompare(skin.string(vis_bg),slideshowmosaic) | StringCompare(skin.string(vis_bg),slideshowmosaicmix)</visible>
