<?xml version="1.0" encoding="UTF-8"?>
<window id="6629" type="dialog"><!-- Aeon MQ 6 -->
	<defaultcontrol always="true">9930</defaultcontrol>
	<onunload condition="!Window.IsActive(Home)">AlarmClock(firstfocus,SetFocus(51),0:01,true)</onunload>
	<onunload condition="Window.IsActive(Home) + ![StringCompare(skin.string(codestep),step1) | StringCompare(skin.string(codestep),step2) | StringCompare(skin.string(codestep),step3)]">AlarmClock(firstfocus,SetFocus(9000),0:01,true)</onunload>
	<onunload condition="Window.IsActive(Home) + StringCompare(skin.string(codestep),step3)">AlarmClock(firstfocus,SetFocus(5100),0:01,true)</onunload>
	<zorder>3</zorder>
	<coordinates>
	<system>1</system>
