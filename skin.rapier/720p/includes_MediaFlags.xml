<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<variable name="MediaFlagTheme">
		<value condition="Skin.HasSetting(UseAltMediaFlagTheme)">white</value>
		<value>default</value>
	</variable>
	
	<variable name="DVDFlagging">
		<value condition="substring(ListItem.FilenameAndPath,hddvd) | substring(ListItem.FilenameAndPath,hd-dvd)">hddvd</value>
		<value condition="substring(ListItem.filename,.iso,right) | substring(ListItem.filename,.img,right) | substring(ListItem.FilenameAndPath,dvd) | substring(ListItem.filename,.ifo,right) | substring(ListItem.filename,.vob,right) | substring(ListItem.FilenameAndPath,dvdrip) | substring(ListItem.FilenameAndPath,dvdr5) | substring(ListItem.FilenameAndPath,.r5) | substring(ListItem.FilenameAndPath,dvdscreener) | substring(ListItem.FilenameAndPath,dvdscr) | substring(ListItem.FilenameAndPath,screener)">dvd</value>
	</variable>
	
