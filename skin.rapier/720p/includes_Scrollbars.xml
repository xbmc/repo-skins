	<variable name="ScrollbarTrailer1">
		<value condition="!Skin.HasSetting(ScrollbarTrailerPlayback) + [Container.Content(Movies) | Container.Content(TVShows)] + !String.IsEmpty(ListItem.Trailer)">PlayMedia($ESCINFO[ListItem.Trailer],noresume)</value>
		<value condition="!Skin.HasSetting(ScrollbarTrailerPlayback) + [Container.Content(Movies) | Container.Content(TVShows)] + System.HasAddon(script.extendedinfo)">RunScript(script.extendedinfo,info=youtubebrowser,id=$INFO[ListItem.Label] trailer)</value>
		<value condition="!Skin.HasSetting(ScrollbarTrailerPlayback) + Container.Content(Episodes) + System.HasAddon(script.extendedinfo)">RunScript(script.extendedinfo,info=youtubebrowser,id=$INFO[ListItem.TVShowTitle] $INFO[ListItem.Label] trailer)</value>
		<value condition="!Skin.HasSetting(ScrollbarTrailerPlayback) + Container.Content(musicvideos) + System.HasAddon(script.extendedinfo)">RunScript(script.extendedinfo,info=youtubebrowser,id=$INFO[ListItem.Artist] $INFO[ListItem.Label])</value>
		<value condition="!Skin.HasSetting(ScrollbarTrailerPlayback) + !System.HasAddon(script.extendedinfo)">InstallAddon(script.extendedinfo)</value>
		<value condition="Skin.HasSetting(ScrollbarTrailerPlayback)">noop</value>
	</variable>
	
	<variable name="ScrollbarTrailer2">
		<value condition="!Skin.HasSetting(ScrollbarTrailerPlayback) + [Container.Content(Movies) | Container.Content(TVShows)] + !String.IsEmpty(ListItem.Trailer)">PlayMedia($ESCINFO[ListItem.Trailer],noresume)</value>
