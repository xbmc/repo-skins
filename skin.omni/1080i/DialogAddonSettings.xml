    <coordinates>
        <system>1</system>
        <left>0</left>
        <top>492</top>
    </coordinates>
    <controls>
        <control type="image">
            <width>1920</width>
            <height>588</height>
            <texture colordiffuse="background">$VAR[CommonBackgroundOverlay]</texture>
        </control>
        <control type="image">
            <top>8</top>
            <width>1920</width>
            <height>580</height>
            <texture colordiffuse="background">dialog.png</texture>
        </control>
        <control type="label" id="20">
            <include>HiddenObject</include>
        </control>
        <!-- left group -->
        <control type="group">
            <left>50</left>
            <top>58</top>
            <control type="grouplist" id="9">
                <description>button area</description>
                <width>350</width>
                <height>480</height>
                <itemgap>0</itemgap>
