<!--Alaska-->
<window id="101">
	<defaultcontrol always="true">10</defaultcontrol>
	<animation type="WindowOpen" reversible="false">
		<effect type="zoom" start="80" end="100" center="auto" delay="200" easing="out" tween="back" time="300" />
		<effect type="fade" delay="200" start="0" end="100" time="300" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="zoom" start="100" end="80" center="auto" easing="in" tween="back" time="300" />
		<effect type="fade" start="100" end="0" time="300" />
	</animation>
	<controls>
		<control type="group">
			<posx>880</posx>
			<posy>90</posy>
			<width>707</width>
			<height>183</height>
			<control type="image">
				<posx>324</posx>
				<posy>12</posy>
				<width>46</width>
				<height>46</height>
				<texture>dialogs/dialog_back.png</texture>
				<colordiffuse>DialogBack</colordiffuse>
			</control>
			<control type="progress" id="404">
				<description>Progressbar</description>
				<info>System.Progressbar</info>
				<posx>330</posx>
				<posy>20</posy>
				<width>35</width>
				<height>28</height>
				<texturebg>dialogs/clear.png</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture>dialogs/grey.png</midtexture>
				<righttexture>-</righttexture>
				<overlaytexture>-</overlaytexture>
				<animation effect="rotate" center="auto" start="-90" end="-90" time="0" condition="true">Conditional</animation>
				<animation type="WindowOpen" reversible="false">
					<effect type="fade" delay="400" start="0" end="100" time="100" />
				</animation>
				<animation type="WindowClose" reversible="false">
					<effect type="fade" start="100" end="0" time="10" />
				</animation>
			</control>
			<control type="image">
				<posx>322</posx>
				<posy>10</posy>
				<width>50</width>
				<height>50</height>
				<texture>dialogs/scanprogress.png</texture>
				<colordiffuse>ProgressFilterColor</colordiffuse>
			</control>
			<control type="label" id="1">
				<description>Heading Label</description>
				<posx>310</posx>
				<posy>15</posy>
				<width>627</width>
				<height>51</height>
				<textcolor>Time</textcolor>
				<align>right</align>
				<font>Font_ProgressDialog</font>
			</control>
			<control type="label" id="2">
				<posx>310</posx>
				<posy>35</posy>
				<width>627</width>
				<height>51</height>
				<align>right</align>
				<font>Font_Date</font>
				<textcolor>clear</textcolor>
				<label>-</label>
				<scroll>true</scroll>
				<visible>false</visible>
			</control>
			<control type="label">
				<posx>310</posx>
				<posy>35</posy>
				<width>627</width>
				<height>51</height>
				<align>right</align>
				<font>Font_Date</font>
				<textcolor>Date</textcolor>
				<label>$INFO[Control.GetLabel(2)]</label>
				<scroll>true</scroll>
			</control>
		</control>
		<control type="button" id="10">
			<description>Cancel</description>
			<posx>-190</posx>
			<posy>-390</posy>
			<label>$LOCALIZE[222]</label>
			<texturefocus border="1">-</texturefocus>
			<texturenofocus border="1">-</texturenofocus>
			<onleft>10</onleft>
			<onright>10</onright>
			<onup>10</onup>
			<ondown>10</ondown>
		</control>
		<include>Object_DebugGrid</include>
		<control type="group">
			<include>Debug</include>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>
	</controls>
</window>