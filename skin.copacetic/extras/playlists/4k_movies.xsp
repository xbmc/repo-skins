<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>4K</name>
    <match>one</match>
    <rule field="videoresolution" operator="greaterthan">
        <value>1080</value>
    </rule>
    <rule field="filename" operator="contains">
        <value>2160p</value>
    </rule>
    <order direction="descending">dateadded</order>
</smartplaylist>
