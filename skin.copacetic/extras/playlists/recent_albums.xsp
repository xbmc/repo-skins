<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="albums">
    <name>Recently played albums</name>
    <match>all</match>
    <rule field="lastplayed" operator="inthelast">
        <value>30 Days</value>
    </rule>
    <order direction="descending">lastplayed</order>
</smartplaylist>
