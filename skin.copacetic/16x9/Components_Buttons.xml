	<!-- Arrows with tails -->
	<include name="Arrow_With_Tail_Template">
		<param name="visible" default="true" />
		<param name="width" default="28" />
		<param name="arrowhead" default="20" />
		<param name="tail" default="34" />
		<param name="thickness" default="2" />
		<param name="basevisible" default="false" />
		<param name="basethickness" default="2" />
		<param name="baseadjustment" default="0" />
		<param name="rotateend" default="45" />
		<param name="color" default="$VAR[Breadcrumb_Label_Color]" />
		<definition>
			<control type="group">
				<visible>$PARAM[visible]</visible>
				<width>100%</width>
				<height>100%</height>
				<control type="group">
					<nested />
					<animation effect="rotate" center="auto" end="$PARAM[rotateend]" time="0" condition="true" reversible="false">Conditional</animation>
					<centerleft>50%</centerleft>
					<centertop>50%</centertop>
					<width>$PARAM[width]</width>
					<height>$PARAM[width]</height>
					<control type="image">
						<top>0</top>
						<height>$PARAM[thickness]</height>
						<right>0</right>
						<width>$PARAM[arrowhead]</width>
						<texture colordiffuse="$PARAM[color]">views/white_100.png</texture>
					</control>
					<control type="image">
						<right>0</right>
						<width>$PARAM[thickness]</width>
						<top>0</top>
						<height>$PARAM[arrowhead]</height>
						<texture colordiffuse="$PARAM[color]">views/white_100.png</texture>
					</control>
					<control type="group">
						<animation effect="rotate" center="auto" end="45" time="0" condition="true" reversible="false">Conditional</animation>
						<centerleft>50%</centerleft>
						<width>$PARAM[tail]</width>
						<centertop>50%</centertop>
						<height>$PARAM[arrowhead]</height>
						<control type="image">
							<centertop>50%</centertop>
							<height>$PARAM[thickness]</height>
							<width>100%</width>
							<texture colordiffuse="$PARAM[color]">views/white_100.png</texture>
						</control>
					</control>
				</control>
				<control type="image">
					<animation effect="slide" end="0,-$PARAM[baseadjustment]" time="0" condition="true" reversible="false">Conditional</animation>
					<visible>$PARAM[basevisible]</visible>
					<centerleft>50%</centerleft>
					<centertop>50%</centertop>
					<width>$PARAM[width]</width>
					<height>$PARAM[basethickness]</height>
					<texture colordiffuse="$PARAM[color]">views/white_100.png</texture>
				</control>
