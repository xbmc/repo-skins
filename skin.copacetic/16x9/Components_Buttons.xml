<includes>

	<!-- Crosses and pluses -->
	<include name="Cross_Template">
		<param name="visible" />
		<param name="width" />
		<param name="sidebar" />
		<param name="thickness" />
		<param name="rotated" default="true" />
		<param name="color" default="$VAR[Breadcrumb_Label_Color]" />
		<definition>
			<control type="group">
				<visible>$PARAM[visible]</visible>
				<nested />
				<width>100%</width>
				<height>100%</height>
				<control type="group">
					<animation effect="rotate" center="auto" end="45" time="0" condition="$PARAM[rotated]" reversible="false">Conditional</animation>
					<centerleft>50%</centerleft>
					<centertop>50%</centertop>
					<width>$PARAM[width]</width>
					<height>$PARAM[width]</height>
					<control type="image">
						<centertop>50%</centertop>
						<height>$PARAM[thickness]</height>
						<width>100%</width>
						<texture colordiffuse="$PARAM[color]">views/white_100.png</texture>
					</control>
					<control type="image">
						<centerleft>50%</centerleft>
						<width>$PARAM[thickness]</width>
						<height>100%</height>
						<texture colordiffuse="$PARAM[color]">views/white_100.png</texture>
					</control>
				</control>
			</control>
		</definition>
	</include>

