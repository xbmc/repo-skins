<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- textviewer -->
	<defaultcontrol always="true">61</defaultcontrol>
	<controls>
		<include content="Default_Dialog_Panel_Layout">
			<include content="Breadcrumb_Label">
				<param name="title_label_id" value="1" />
			</include>
			<include content="Description_Label">
				<param name="id" value="5" />
				<param name="type" value="textbox" />
				<param name="textcolor" value="$VAR[Light_Color]" />
				<param name="left" value="120" />
				<param name="right" value="120" />
				<param name="height" value="645" />
				<param name="bottom" value="218" />
				<pagecontrol>61</pagecontrol>
				<autoscroll>false</autoscroll>
			</include>
			<control type="scrollbar" id="61">
				<visible allowhiddenfocus="true">true</visible>
				<right>26</right>
				<width>6</width>
				<centertop>50%</centertop>
				<height>720</height>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
				<texturesliderbackground colordiffuse="$VAR[Scroll_Background_Color]">views/white_100.png</texturesliderbackground>
				<texturesliderbarfocus colordiffuse="$VAR[Scroll_Focused_Color]">views/white_100.png</texturesliderbarfocus>
			</control>
		</include>
	</controls>
</window>