<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<default type="label">
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>0</textoffsetx>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<disabledcolor>$VAR[Dark_Color]</disabledcolor>
		<selectedcolor>$VAR[Accent_Color]</selectedcolor>
	</default>

	<default type="textbox">
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>0</textoffsetx>
		<textcolor>$VAR[Light_Color]</textcolor>
		<autoscroll delay="4000" time="4000" repeat="12000">true</autoscroll>
	</default>

	<default type="button">
		<width>100%</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>60</textoffsetx>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Light_Color]</focusedcolor>
		<invalidcolor>$VAR[Invalid_Color]</invalidcolor>
		<disabledcolor>$VAR[Disabled_Color]</disabledcolor>
		<texturenofocus/>
		<texturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</texturefocus>
<!--<texturefocus border="2,2,2,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_focused.png</texturefocus>-->
<!--<texturefocus border="62,2,2,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_left.png</texturefocus>-->
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="colorbutton">
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>60</textoffsetx>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Light_Color]</focusedcolor>
		<invalidcolor>$VAR[Invalid_Color]</invalidcolor>
		<disabledcolor>$VAR[Disabled_Color]</disabledcolor>
		<colorwidth>180</colorwidth>
		<colorheight>60</colorheight>
		<colorposx>1020</colorposx>
		<texturecolormask>dialogs/color_mask.png</texturecolormask>
		<texturecolordisabledmask colordiffuse="$VAR[Unfocused_Color]">dialogs/color_mask.png</texturecolordisabledmask>
		<texturenofocus/>
		<texturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</texturefocus>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="radiobutton">
		<width>100%</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textwidth>990</textwidth>
		<textoffsetx>60</textoffsetx>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Light_Color]</focusedcolor>
		<disabledcolor>$VAR[Disabled_Color]</disabledcolor>
		<radiowidth>60</radiowidth>
		<radioheight>90</radioheight>
		<radioposx>1140</radioposx>
		<texturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</texturefocus>
		<textureradioonfocus colordiffuse="$VAR[Light_Color]">dialogs/Radio_On_Focused.png</textureradioonfocus>
		<textureradioofffocus colordiffuse="$VAR[Light_Color]">dialogs/Radio_Off_Focused.png</textureradioofffocus>
		<textureradioonnofocus colordiffuse="$VAR[Unfocused_Color]">dialogs/Radio_On_Unfocused.png</textureradioonnofocus>
		<textureradiooffnofocus colordiffuse="$VAR[Unfocused_Color]">dialogs/Radio_Off_Unfocused.png</textureradiooffnofocus>
		<textureradioondisabled colordiffuse="$VAR[Unfocused_Color]">dialogs/Radio_On_Unfocused.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="$VAR[Unfocused_Color]">dialogs/Radio_Off_Unfocused.png</textureradiooffdisabled>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="spincontrolex">
		<width>100%</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>60</textoffsetx>
		<textwidth>780</textwidth>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Light_Color]</focusedcolor>
		<disabledcolor>$VAR[Disabled_Color]</disabledcolor>
		<spinwidth>60</spinwidth>
		<spinheight>90</spinheight>
		<texturenofocus/>
		<texturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</texturefocus>
		<textureup colordiffuse="$VAR[Unfocused_Color]">dialogs/spin_unfocused.png</textureup>
		<textureupfocus colordiffuse="$VAR[Light_Color]">dialogs/spin_focused.png</textureupfocus>
		<textureupdisabled/>
		<texturedown colordiffuse="$VAR[Unfocused_Color]" flipy="true">dialogs/spin_unfocused.png</texturedown>
		<texturedownfocus colordiffuse="$VAR[Light_Color]" flipy="true">dialogs/spin_focused.png</texturedownfocus>
		<texturedowndisabled/>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="sliderex">
		<width>100%</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textwidth>780</textwidth>
		<textoffsetx>60</textoffsetx>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Light_Color]</focusedcolor>
		<disabledcolor>$VAR[Disabled_Color]</disabledcolor>
		<texturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</texturefocus>
		<texturenofocus/>
		<sliderwidth>120</sliderwidth>
		<sliderheight>30</sliderheight>
		<orientation>vertical</orientation>
		<texturesliderbar border="3,3,3,3" colordiffuse="$VAR[Unfocused_Color]">dialogs/slider_background.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[Unfocused_Color]">dialogs/slider_nib_unfocused.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[Light_Color]">dialogs/slider_nib_focused.png</textureslidernibfocus>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="slider">
		<width>500</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>60</textoffsetx>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Light_Color]</focusedcolor>
		<disabledcolor>$VAR[Disabled_Color]</disabledcolor>
		<texturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</texturefocus>
		<texturenofocus/>
		<sliderwidth>120</sliderwidth>
		<sliderheight>30</sliderheight>
		<orientation>vertical</orientation>
		<texturesliderbar border="3,3,3,3" colordiffuse="$VAR[Unfocused_Color]">dialogs/slider_background.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[Unfocused_Color]">dialogs/slider_nib_unfocused.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[Light_Color]">dialogs/slider_nib_focused.png</textureslidernibfocus>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="edit">
		<width>100%</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>60</textoffsetx>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Light_Color]</focusedcolor>
		<disabledcolor>$VAR[Disabled_Color]</disabledcolor>
		<invalidcolor>$VAR[Invalid_Color]</invalidcolor>
		<scroll>true</scroll>
		<scrollsuffix> / </scrollsuffix>
		<hinttext/>
		<texturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</texturefocus>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="progress">
		<width/>
		<height/>
		<texturebg/>
		<lefttexture/>
		<midtexture/>
		<righttexture/>
		<overlaytexture/>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="togglebutton">
		<width>100%</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textwidth>990</textwidth>
		<textoffsetx>60</textoffsetx>
		<textcolor>$VAR[Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Light_Color]</focusedcolor>
		<disabledcolor>$VAR[Disabled_Color]</disabledcolor>
		<radiowidth>90</radiowidth>
		<radioheight>90</radioheight>
		<radioposx>1065</radioposx>
		<texturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</texturefocus>
		<texturenofocus/>
		<alttexturefocus border="2,2,62,2" colordiffuse="$VAR[Unfocused_Color]">dialogs/button_underline_except_60_right.png</alttexturefocus>
		<alttexturenofocus/>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="mover">
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="resize">
		<pulseonselect>no</pulseonselect>
	</default>

</includes>