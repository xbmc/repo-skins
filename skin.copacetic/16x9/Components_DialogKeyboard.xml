					<width>100%</width>
					<bottom>0</bottom>
					<height>1</height>
					<texture colordiffuse="$VAR[Unfocused_Color]">views/white_100.png</texture>
				</control>
			</focusedlayout>
			<content>$VAR[AutoComplete_Path]</content>
