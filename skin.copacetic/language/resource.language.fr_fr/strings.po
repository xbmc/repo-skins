msgctxt "#31480"
msgid "Enable background fanart"
msgstr "Activer l'arrière-plan fanart"

# /16x9/Content_Settings.xml
msgctxt "#31481"
msgid "Visualisation screen album cover"
msgstr "Écran de visualisation pochette d'album"

