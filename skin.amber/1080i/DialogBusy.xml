<?xml version="1.0" encoding="UTF-8"?>
<window>
	<controls>
		<include>DimBG</include>
		<control type="group">
			<visible>String.IsEmpty(Window(Home).Property(script.cinemavision.running))</visible>
			<visible>!Window.IsActive(startup)</visible>
			<control type="progress" id="10" description="Progress bar">
				<posx>708</posx>
				<posy>550</posy>
				<width>150</width>
				<height>8</height>
				<texturebg></texturebg>
				<midtexture></midtexture>
				<righttexture></righttexture>
				<lefttexture></lefttexture>
			</control>
			<control type="group" description="Content">
				<animation effect="slide" start="0,0" end="0,-124" time="200">WindowOpen</animation>
				<posy>604</posy>
				<control type="image" description="Background">
					<posx>708</posx>
					<posy>546</posy>
					<width>504</width>
					<height>54</height>
					<texture border="48,24,48,0">dialogs/BusyBG.png</texture>
					<colordiffuse>$VAR[DialogBGColor]</colordiffuse>
				</control>
				<control type="label" id="1" description="Progress label">
					<posx>708</posx>
					<posy>539</posy>
					<width>504</width>
					<height>70</height>
					<align>center</align>
					<textcolor>$VAR[HighlightColor]</textcolor>
					<label>$VAR[DialogBusyLabel]</label>
				</control>
			</control>
		</control>
	</controls>
</window>