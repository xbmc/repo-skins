<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">2</defaultcontrol>
	<backgroundcolor>0</backgroundcolor>
	<controls>
		<include>DefaultBackground</include>
		<include>Time</include>
		<control type="group" description="Content">
			<include>DialogOpenClose</include>
			<posx>560</posx>
			<posy>180</posy>
			<control type="image">
				<posx>-17</posx>
				<width>834</width>
				<height>650</height>
				<texture border="45">dialogs/TinyDialog.png</texture>
				<colordiffuse>$VAR[DialogBGColor]</colordiffuse>
			</control>
			<control type="label" description="Heading">
				<posy>24</posy>
				<width>800</width>
				<height>40</height>
				<align>center</align>
				<textcolor>$VAR[HighlightColor]</textcolor>
				<label>$LOCALIZE[10034]</label>
			</control>
			<control type="radiobutton" id="4" description="Enable Login Screen">
				<posy>90</posy>
				<width>800</width>
				<label>$LOCALIZE[20096]</label>
				<onright>noop</onright>
				<onleft>noop</onleft>
				<onup>noop</onup>
				<ondown>5</ondown>
				<textoffsetx>30</textoffsetx>
			</control>
			<control type="radiobutton" id="5" description="Enable auto Login">
				<posy>150</posy>
				<width>800</width>
				<label>$LOCALIZE[33084]$INFO[system.profileautologin,:  ]</label>
				<onright>noop</onright>
				<onleft>noop</onleft>
				<onup>4</onup>
				<ondown>2</ondown>
				<textoffsetx>30</textoffsetx>
			</control>
			<control type="group" description="Slider and List">
				<posy>240</posy>
				<control type="list" id="2">
					<width>800</width>
					<height>360</height>
					<onleft>noop</onleft>
					<onright>noop</onright>
					<onup>5</onup>
					<ondown>noop</ondown>
					<pagecontrol>60</pagecontrol>
					<scrolltime>280</scrolltime>
					<itemlayout width="800" height="60">
						<control type="image">
							<width>800</width>
							<height>60</height>
							<texture border="3" flipy="true">list/ListNF.png</texture>
						</control>
						<control type="label" description="Label2">
							<posx>30r</posx>
							<height>60</height>
							<width>800</width>
							<font>ListDetails</font>
							<scroll>false</scroll>
							<align>right</align>
							<label>$INFO[ListItem.Label2]</label>
						</control>
						<control type="label" description="Main Label">
							<height>60</height>
							<width>800</width>
							<textoffsetx>30</textoffsetx>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</itemlayout>
					<focusedlayout width="800" height="60">
						<control type="image">
							<width>800</width>
							<height>60</height>
							<texture border="3" flipy="true">list/ListNF.png</texture>
						</control>
						<control type="image">
							<visible>![Control.HasFocus(4) | Control.HasFocus(5)]</visible>
							<width>800</width>
							<height>60</height>
							<texture border="4" colordiffuse="$VAR[HighlightColor]">img/WhiteRoundedST2.png</texture>
						</control>
						<control type="label" description="Label2">
							<posx>30r</posx>
							<height>60</height>
							<width>800</width>
							<font>ListDetails</font>
							<align>right</align>
							<label>$INFO[ListItem.Label2]</label>
							<visible>![Control.HasFocus(4) | Control.HasFocus(5)]</visible>
						</control>
						<control type="label" description="Label2">
							<posx>30r</posx>
							<height>60</height>
							<width>800</width>
							<font>ListDetails</font>
							<textcolor>$VAR[FocusedTextColor]</textcolor>
							<shadowcolor></shadowcolor>
							<align>right</align>
							<label>$INFO[ListItem.Label2]</label>
							<visible>[Control.HasFocus(4) | Control.HasFocus(5)]</visible>
						</control>
						<control type="label" description="Main Label">
							<height>60</height>
							<width>800</width>
							<textoffsetx>30</textoffsetx>
							<label>$INFO[ListItem.Label]</label>
							<visible>[Control.HasFocus(4) | Control.HasFocus(5)]</visible>
						</control>
						<control type="label" description="Main Label">
							<height>60</height>
							<width>800</width>
							<textoffsetx>30</textoffsetx>
							<textcolor>$VAR[FocusedTextColor]</textcolor>
							<shadowcolor></shadowcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>![Control.HasFocus(4) | Control.HasFocus(5)]</visible>
						</control>
					</focusedlayout>
				</control>
			</control>
		</control>
	</controls>
</window>