<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>61</defaultcontrol>
	<include>OpenClose</include>
	<controls>
		<include>DimBG</include>
		<control type="group" description="Content">
			<include>DialogOpenClose</include>
			<control type="image">
				<posx>360</posx>
				<posy>20</posy>
				<width>1200</width>
				<height>1040</height>
				<texture>dialogs/DialogBG.png</texture>
				<colordiffuse>$VAR[DialogBGColor]</colordiffuse>
			</control>
			<control type="label" id="1" description="Header">
				<posy>60</posy>
				<width>1920</width>
				<height>45</height>
				<align>center</align>
				<textcolor>$VAR[HighlightColor]</textcolor>
			</control>
			<control type="textbox" id="5" description="Textarea">
				<posx>424</posx>
				<posy>150</posy>
				<width>1070</width>
				<height>840</height>
				<font>InfoPlot</font>
				<align>justify</align>
                <pagecontrol>61</pagecontrol>
			</control>
			<control type="scrollbar" id="61">
				<posx>1523</posx>
				<posy>150</posy>
				<width>36</width>
				<height>840</height>
				<texturesliderbackground></texturesliderbackground>
				<texturesliderbar></texturesliderbar>
				<texturesliderbarfocus></texturesliderbarfocus>
				<textureslidernib border="18">OSD/ScrollbarNibNF.png</textureslidernib>
				<textureslidernibfocus border="18" colordiffuse="$VAR[HighlightColor]">OSD/ScrollbarNibFO.png</textureslidernibfocus>
