<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="label">
		<height>60</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>font18</font>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<shadowcolor>ShadowDark</shadowcolor>
		<selectedcolor>$VAR[SelectedTextColor]</selectedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<scroll>true</scroll>
		<scrollspeed>25</scrollspeed>
		<scrollsuffix>  •  </scrollsuffix>
	</default>
	<default type="fadelabel">
		<height>60</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>font18</font>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<shadowcolor>ShadowDark</shadowcolor>
		<focusedcolor>$VAR[LabelTextColor]</focusedcolor>
		<selectedcolor>$VAR[SelectedTextColor]</selectedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<scrollout>false</scrollout>
		<pauseatend>3000</pauseatend>
		<scrollsuffix>  •  </scrollsuffix>
	</default>
	<default type="textbox">
		<align>justify</align>
		<font>InfoPlot</font>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<shadowcolor>ShadowDark</shadowcolor>
		<autoscroll delay="5000" time="2500" repeat="10000">Skin.HasSetting(AutoScroll)</autoscroll>
	</default>
	<default type="image">
		<align>center</align>
		<aligny>center</aligny>
		<aspectratio>stretch</aspectratio>
	</default>
	<default type="button">
		<width>390</width>
		<height>65</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>font18</font>
		<textoffsetx>20</textoffsetx>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<focusedcolor>$VAR[FocusedTextColor]</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<texturefocus border="4" colordiffuse="$VAR[HighlightColor]">img/WhiteRoundedST2.png</texturefocus>
		<texturenofocus border="4">img/WhiteRoundedT.png</texturenofocus>
		<bordersize>4</bordersize>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="edit">
		<width>390</width>
		<height>60</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>font18</font>
		<textoffsetx>20</textoffsetx>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<focusedcolor>$VAR[FocusedTextColor]</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<texturefocus border="4" colordiffuse="$VAR[HighlightColor]">img/WhiteRoundedST2.png</texturefocus>
		<texturenofocus border="4">img/WhiteRoundedT.png</texturenofocus>
	</default>
	<default type="togglebutton">
		<width>450</width>
		<height>60</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>font18</font>
		<textoffsetx>20</textoffsetx>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<focusedcolor>$VAR[FocusedTextColor]</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<texturefocus border="4" colordiffuse="$VAR[HighlightColor]">img/WhiteRoundedST2.png</texturefocus>
		<texturenofocus border="3">buttons/ButtonNF.png</texturenofocus>
		<alttexturefocus border="4" colordiffuse="$VAR[HighlightColor]">img/WhiteRoundedST2.png</alttexturefocus>
		<alttexturenofocus border="3">buttons/ButtonNF.png</alttexturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="radiobutton">
		<height>60</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>font18</font>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<focusedcolor>$VAR[FocusedTextColor]</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<radiowidth>80</radiowidth>
		<radioheight>60</radioheight>
		<texturefocus border="4" colordiffuse="$VAR[HighlightColor]">img/WhiteRoundedST2.png</texturefocus>
		<texturenofocus border="3">buttons/ButtonNF.png</texturenofocus>
		<textureradioonfocus colordiffuse="$VAR[HighlightColor]">buttons/RadioOn.png</textureradioonfocus>
		<textureradioonnofocus colordiffuse="$VAR[HighlightColor]">buttons/RadioOn.png</textureradioonnofocus>
		<textureradioofffocus>buttons/RadioOff.png</textureradioofffocus>
		<textureradiooffnofocus>buttons/RadioOff.png</textureradiooffnofocus>
		<textoffsetx>20</textoffsetx>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrol">
		<height>60</height>
		<textureup>buttons/SpinUpNF.png</textureup>
		<textureupfocus colordiffuse="$VAR[HighlightColor]">buttons/SpinUpFO.png</textureupfocus>
		<texturedown>buttons/SpinDownNF.png</texturedown>
		<texturedownfocus colordiffuse="$VAR[HighlightColor]">buttons/SpinDownFO.png</texturedownfocus>
		<spinwidth>40</spinwidth>
		<spinheight>60</spinheight>
		<font>font18</font>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<focusedcolor>$VAR[FocusedTextColor]</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<textoffsetx>20</textoffsetx>
		<align>left</align>
		<aligny>center</aligny>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrolex">
		<spinposx>7</spinposx>
		<height>60</height>
		<font>font18</font>
		<textureup>buttons/SpinUpNF.png</textureup>
		<textureupfocus colordiffuse="$VAR[HighlightColor]">buttons/SpinUpFO.png</textureupfocus>
		<texturedown>buttons/SpinDownNF.png</texturedown>
		<texturedownfocus colordiffuse="$VAR[HighlightColor]">buttons/SpinDownFO.png</texturedownfocus>
		<texturefocus border="4" colordiffuse="$VAR[HighlightColor]">img/WhiteRoundedST2.png</texturefocus>
		<texturenofocus border="3">buttons/ButtonNF.png</texturenofocus>
		<spinwidth>40</spinwidth>
		<spinheight>60</spinheight>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<focusedcolor>$VAR[FocusedTextColor]</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<textoffsetx>20</textoffsetx>
		<align>left</align>
		<aligny>center</aligny>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="slider" description="Default Slider">
		<align>left</align>
		<aligny>center</aligny>
		<visible>true</visible>
		<height>60</height>
		<orientation>horizontal</orientation>
		<sliderwidth>180</sliderwidth>
		<sliderheight>60</sliderheight>
		<texturesliderbar>buttons/SliderBG.png</texturesliderbar>
		<textureslidernib>buttons/SliderNibNF.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[HighlightColor]">buttons/SliderNibFO.png</textureslidernibfocus>
		<textoffsetx>20</textoffsetx>
	</default>
	<default type="grouplist">
		<itemgap>0</itemgap>
		<align>left</align>
	</default>
	<default type="sliderex" description="Default Slider">
		<align>left</align>
		<aligny>center</aligny>
		<height>60</height>
		<sliderwidth>180</sliderwidth>
		<sliderheight>60</sliderheight>
		<texturesliderbar>buttons/SliderBG.png</texturesliderbar>
		<textureslidernib>buttons/SliderNibNF.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[HighlightColor]">buttons/SliderNibFO.png</textureslidernibfocus>
		<texturefocus border="4" colordiffuse="$VAR[HighlightColor]">img/WhiteRoundedST2.png</texturefocus>
		<texturenofocus border="3">buttons/ButtonNF.png</texturenofocus>
		<font>font18</font>
		<textcolor>$VAR[LabelTextColor]</textcolor>
		<focusedcolor>$VAR[FocusedTextColor]</focusedcolor>
		<disabledcolor>Disabled</disabledcolor>
		<textoffsetx>20</textoffsetx>
	</default>
	<default type="scrollbar">
		<width>9</width>
		<orientation>vertical</orientation>
		<texturesliderbackground border="0,4,0,4">Scrollbar/ScrollbarBG.png</texturesliderbackground>
		<texturesliderbar border="0,4,0,4">Scrollbar/ScrollerNF.png</texturesliderbar>
		<texturesliderbarfocus border="0,4,0,4" colordiffuse="$VAR[HighlightColor]">Scrollbar/ScrollerFO.png</texturesliderbarfocus>
		<pulseonselect>false</pulseonselect>
		<showonepage>false</showonepage>
	</default>
</includes>