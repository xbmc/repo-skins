<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<includes>

    <constant name="NavBarPad">112</constant>
    <constant name="SidePad">80</constant>
    <constant name="PosterPad">216</constant>
    <constant name="PosterH">640</constant>
    <constant name="PosterW">428</constant>
    <constant name="PosterShadowH">688</constant>
    <constant name="PosterShadowW">475</constant>
    <constant name="ListX">540</constant>
    <constant name="ListW">1300</constant>
    <constant name="ListSquareX">752</constant>
    <constant name="ListSquareW">1088</constant>

    <default type="label">
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>dialog_fg_70</focusedcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <disabledcolor>dialog_fg_30</disabledcolor>
    </default>

    <default type="edit">
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>Selected</focusedcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <disabledcolor>dialog_fg_12</disabledcolor>
        <texturefocus colordiffuse="dialog_bg_30">common/white.png</texturefocus>
        <texturenofocus/>
        <pulseonselect>false</pulseonselect>
    </default>

    <default type="fadelabel">
        <align>left</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>Selected</focusedcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <disabledcolor>dialog_fg_12</disabledcolor>
        <scrollout>false</scrollout>
        <pauseatend>2000</pauseatend>
    </default>

    <default type="image">
    </default>

    <default type="button">
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>Selected</focusedcolor>
        <disabledcolor>dialog_fg_30</disabledcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <texturefocus colordiffuse="dialog_bg_12">common/white.png</texturefocus>
        <texturenofocus/>
        <pulseonselect>false</pulseonselect>
    </default>

    <default type="togglebutton">
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>Selected</focusedcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <disabledcolor>dialog_fg_30</disabledcolor>
        <texturefocus colordiffuse="dialog_bg_12">common/white.png</texturefocus>
        <texturenofocus/>
        <alttexturefocus colordiffuse="dialog_bg_12">common/white.png</alttexturefocus>
        <alttexturenofocus/>
        <pulseonselect>false</pulseonselect>
    </default>

    <default type="radiobutton">
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <radioposx>1250</radioposx>
        <radiowidth>32</radiowidth>
        <radioheight>32</radioheight>
        <textoffsetx>30</textoffsetx>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>Selected</focusedcolor>
        <disabledcolor>dialog_fg_12</disabledcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <texturefocus colordiffuse="dialog_bg_12">common/white.png</texturefocus>
        <texturenofocus/>
        <textureradiofocus colordiffuse="dialog_fg_70">buttons/radio-on.png</textureradiofocus>
        <textureradionofocus colordiffuse="dialog_fg_30">buttons/radio-off.png</textureradionofocus>
        <textureradioondisabled colordiffuse="dialog_fg_12">buttons/radio-on.png</textureradioondisabled>
        <textureradiooffdisabled colordiffuse="dialog_fg_12">buttons/radio-off.png</textureradiooffdisabled>
        <textoffsetx>30</textoffsetx>
        <pulseonselect>false</pulseonselect>
    </default>

    <default type="spincontrolex">
        <font>Small</font>
        <spinwidth>32</spinwidth>
        <spinheight>32</spinheight>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>Selected</focusedcolor>
        <disabledcolor>dialog_fg_12</disabledcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <texturefocus colordiffuse="dialog_bg_12">common/white.png</texturefocus>
        <texturenofocus/>
        <textureup flipy="true" colordiffuse="dialog_fg_30">buttons/spin-down.png</textureup>
        <texturedown colordiffuse="dialog_fg_30">buttons/spin-down.png</texturedown>
        <textureupfocus flipy="true" colordiffuse="Selected">buttons/spin-down.png</textureupfocus>
        <texturedownfocus colordiffuse="Selected">buttons/spin-down.png</texturedownfocus>
        <textureupdisabled flipy="true" colordiffuse="dialog_fg_12">buttons/spin-down.png</textureupdisabled>
        <texturedowndisabled colordiffuse="dialog_fg_12">buttons/spin-down.png</texturedowndisabled>
        <textoffsetx>30</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>

    <default type="spincontrol">
        <font>Small</font>
        <spinwidth>32</spinwidth>
        <spinheight>32</spinheight>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>Selected</focusedcolor>
        <disabledcolor>dialog_fg_12</disabledcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <texturefocus colordiffuse="dialog_bg_12">common/white.png</texturefocus>
        <texturenofocus/>
        <textureup flipy="true" colordiffuse="dialog_fg_30">buttons/spin-down.png</textureup>
        <texturedown colordiffuse="dialog_fg_30">buttons/spin-down.png</texturedown>
        <textureupfocus flipy="true" colordiffuse="Selected">buttons/spin-down.png</textureupfocus>
        <texturedownfocus colordiffuse="Selected">buttons/spin-down.png</texturedownfocus>
        <textureupdisabled flipy="true" colordiffuse="dialog_fg_12">buttons/spin-down.png</textureupdisabled>
        <texturedowndisabled colordiffuse="dialog_fg_12">buttons/spin-down.png</texturedowndisabled>
        <textoffsetx>30</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>



    <default type="progress">
        <texturebg colordiffuse="2f5f5f5f">osd/progress-bg.png</texturebg>
        <lefttexture colordiffuse="$VAR[ColorHighlight]">osd/progress-end.png</lefttexture>
        <midtexture colordiffuse="$VAR[ColorHighlight]">osd/progress-end.png</midtexture>
        <righttexture colordiffuse="$VAR[ColorHighlight]">osd/progress-end.png</righttexture>
        <overlaytexture/>
    </default>

    <default type="slider">
        <width>360</width>
        <height>23</height>
        <orientation>horizontal</orientation>
        <texturefocus>-</texturefocus>
        <texturenofocus>-</texturenofocus>
        <texturesliderbar>other_textures/OSD/slider_bg_2.png</texturesliderbar>
        <textureslidernib border="2">other_textures/OSD/SliderNibNF.png</textureslidernib>
        <textureslidernibfocus border="2">other_textures/OSD/SliderNibFO.png</textureslidernibfocus>
    </default>

    <default type="sliderex">
        <orientation>horizontal</orientation>
        <selectedcolor>Highlight2</selectedcolor>
        <sliderwidth>230</sliderwidth>
        <sliderheight>23</sliderheight>
        <texturesliderbar>other_textures/OSD/slider_bg.png</texturesliderbar>
        <textureslidernib border="2">other_textures/OSD/SliderNibNF.png</textureslidernib>
        <textureslidernibfocus border="2">other_textures/OSD/SliderNibFO.png</textureslidernibfocus>
        <font>Small</font>
        <aligny>center</aligny>
        <textcolor>dialog_fg_70</textcolor>
        <focusedcolor>Selected</focusedcolor>
        <disabledcolor>dialog_fg_12</disabledcolor>
        <textoffsetx>30</textoffsetx>
        <textoffsety>0</textoffsety>
    </default>

    <default type="scrollbar">
        <width>8</width>
        <texturesliderbackground colordiffuse="dialog_bg_12">osd/progress-end.png</texturesliderbackground>
        <texturesliderbar colordiffuse="dialog_bg_30">osd/progress-end.png</texturesliderbar>
        <texturesliderbarfocus colordiffuse="$VAR[ColorHighlight]">osd/progress-end.png</texturesliderbarfocus>
        <textureslidernib/>
        <textureslidernibfocus/>
        <showonepage>false</showonepage>
        <orientation>vertical</orientation>
    </default>



    <default type="textbox">
        <font>Small</font>
        <textcolor>dialog_fg_70</textcolor>
        <shadowcolor/>
        <selectedcolor>Highlight2</selectedcolor>
        <pagecontrol>61</pagecontrol>
        <autoscroll delay="6000" time="3000" repeat="10000"/>
    </default>

</includes>
