<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="label">
		<font>font13</font>
		<scroll>false</scroll>
		<scrollsuffix> • </scrollsuffix>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<aligny>center</aligny>
	</default>
	<default type="button">
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<font>font13</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<invalidcolor>invalid-text</invalidcolor>
		<pulseonselect>false</pulseonselect>
		<textoffsetx>18</textoffsetx>
		<aligny>center</aligny>
	</default>
	<default type="togglebutton">
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<alttexturefocus border="7">button-default-focus.png</alttexturefocus>
		<font>font13</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<pulseonselect>false</pulseonselect>
		<textoffsetx>18</textoffsetx>
		<aligny>center</aligny>
	</default>
	<default type="fadelabel">
		<font>font13</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<scroll>false</scroll>
		<scrollout>false</scrollout>
		<pauseatend>4000</pauseatend>
		<scrollsuffix> • </scrollsuffix>
		<aligny>center</aligny>
	</default>
	<default type="textbox">
		<font>font13</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<autoscroll time="4000" delay="6000" repeat="5000">Skin.HasSetting(text-autoscroll) + !System.ScreenSaverActive</autoscroll>
	</default>
	<default type="progress">
		<texturebg border="8,0,8,0">progress-background.png</texturebg>
		<lefttexture>-</lefttexture>
		<midtexture border="8,0,8,0">progress-bar-focus.png</midtexture>
		<righttexture>-</righttexture>
		<overlaytexture/>
		<colordiffuse>ProgressBarColor</colordiffuse>
	</default>
	<default type="scrollbar">
		<texturesliderbackground border="8">scrollbar_background.png</texturesliderbackground>
		<texturesliderbar border="8">scrollbar_bar.png</texturesliderbar>
		<texturesliderbarfocus border="8">scrollbar_bar_focus.png</texturesliderbarfocus>
		<showonepage>false</showonepage>
		<pulseonselect>false</pulseonselect>
		<colordiffuse>ScrollBarColor</colordiffuse>
	</default>
	<default type="spincontrol">
		<textureup>scroll-up.png</textureup>
		<texturedown>scroll-down.png</texturedown>
		<textureupfocus>scroll-up-focus.png</textureupfocus>
		<texturedownfocus>scroll-down-focus.png</texturedownfocus>
		<textureupdisabled>scroll-up.png</textureupdisabled>
		<texturedowndisabled>scroll-down.png</texturedowndisabled>
		<align>right</align>
		<width>32</width>
		<height>32</height>
		<font>font13</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<pulseonselect>false</pulseonselect>
		<subtype>text</subtype>
	</default>
	<default type="spincontrolex">
		<spinwidth>32</spinwidth>
		<spinheight>32</spinheight>
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<textureup>scroll-up.png</textureup>
		<texturedown>scroll-down.png</texturedown>
		<textureupfocus>scroll-up-focus.png</textureupfocus>
		<texturedownfocus>scroll-down-focus.png</texturedownfocus>
		<textureupdisabled>scroll-up.png</textureupdisabled>
		<texturedowndisabled>scroll-down.png</texturedowndisabled>
		<font>font13</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<pulseonselect>false</pulseonselect>
		<textoffsetx>18</textoffsetx>
		<aligny>center</aligny>
		<reverse>yes</reverse>
	</default>
	<default type="radiobutton">
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<radiowidth/>
		<radioheight/>
		<font>font13</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<pulseonselect>false</pulseonselect>
		<textoffsetx>18</textoffsetx>
		<aligny>center</aligny>
	</default>
	<default type="selectbutton">
		<font>font13</font>
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<texturebg>button-menu-background.png</texturebg>
		<textureleft>scroll-left.png</textureleft>
		<textureleftfocus>scroll-left-focus.png</textureleftfocus>
		<textureright>scroll-right.png</textureright>
		<texturerightfocus>scroll-right-focus.png</texturerightfocus>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<pulseonselect>false</pulseonselect>
		<textoffsetx>18</textoffsetx>
		<aligny>center</aligny>
	</default>
	<default type="slider">
		<texturesliderbar>blank.png</texturesliderbar>
		<textureslidernib>progress-marker.png</textureslidernib>
		<textureslidernibfocus>progress-marker.png</textureslidernibfocus>
		<pulseonselect>false</pulseonselect>
		<controloffsetx>75</controloffsetx>
		<controloffsety>0</controloffsety>
	</default>
	<default type="sliderex">
		<font>font13</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<texturesliderbar colordiffuse="ProgressBarColor" border="8">progress-bar-focus.png</texturesliderbar>
		<textureslidernib>progress-marker.png</textureslidernib>
		<textureslidernibfocus>progress-marker.png</textureslidernibfocus>
		<pulseonselect>false</pulseonselect>
		<sliderwidth>200</sliderwidth>
		<sliderheight>16</sliderheight>
		<textoffsetx>18</textoffsetx>
		<align>left</align>
		<aligny>center</aligny>
	</default>
	<default type="edit">
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<font>font12</font>
		<textcolor>button-text</textcolor>
		<shadowcolor>shadow-text</shadowcolor>
		<disabledcolor>disabled-text</disabledcolor>
		<invalidcolor>invalid-text</invalidcolor>
		<pulseonselect>false</pulseonselect>
		<textoffsetx>18</textoffsetx>
		<aligny>center</aligny>
	</default>
	<default type="mover">
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="resize">
		<texturefocus border="7">button-default-focus.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>
</includes>