   <include name="MenuBackground">
     <param name="border" value="40" />
	 <param name="color" default="$VAR[MenuColor]"/>
	 <param name="height" default="0"/>
	 <param name="posx" default="0"/>
	 <param name="posy" default="0"/>
	 <param name="texture" default="buttons/types/bd.png"/>
	 <param name="width" default="0"/>
	 
	 <param name="condition" default="!Skin.HasSetting(MenuBackground)"/>
	 <param name="type0" default="true" description="MenuBackground"/>
	 <param name="type1" default="false" description="DialogBackground"/>
	 
	 <definition>
		<control type="group">
		  <height>$PARAM[height]</height>
		  <posx>$PARAM[posx]</posx>
		  <posy>$PARAM[posy]</posy>
		  <width>$PARAM[width]</width>
		  <visible>$PARAM[condition]</visible>
		  
		  <control type="image" description="type0"> 
			<include>Border_Common</include>
			<include>Fade_MenuBackgroundOverlay</include>
			<height></height>
			<posx></posx>
			<width></width>
			<texture border="$PARAM[border]" colordiffuse="$PARAM[color]">$PARAM[texture]</texture>
			<visible>$PARAM[type0]</visible>
		  </control>
		  
		  <control type="image" description="type1"> 
			<include>Border_Common</include>
			<include>Fade_DialogBackgroundOverlay</include>
			<height></height>
			<posx></posx>
			<width></width>
			<texture border="$PARAM[border]" colordiffuse="$PARAM[color]">$PARAM[texture]</texture>
			<visible>$PARAM[type1]</visible>
		  </control>
		</control>
	</definition>
  </include>
  
